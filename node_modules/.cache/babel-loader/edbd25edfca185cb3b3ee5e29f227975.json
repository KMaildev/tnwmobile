ocomplete=\"on\"\n                  ></v-text-field>\n\n                  <v-select\n                    :items=\"propertytype\"\n                    item-text=\"property_type_mm\"\n                    item-value=\"property_type_id\"\n                    label=\"Property Type\"\n                    v-model=\"propertyDetail.property_type_id\"\n                    required\n                  ></v-select>\n\n                  <v-row>\n                    <v-col cols=\"6\" sm=\"6\">\n                      <v-text-field\n                        type=\"number\"\n                        label=\"Price\"\n                        v-model=\"propertyDetail.price\"\n                        autocomplete=\"on\"\n                        required\n                      ></v-text-field>\n                    </v-col>\n\n                    <v-col cols=\"6\" sm=\"6\">\n                      <v-select\n                        :items=\"currency\"\n                        item-text=\"name\"\n                        item-value=\"value\"\n                        v-model=\"propertyDetail.price_type\"\n                      ></v-select>\n                    </v-col>\n                  </v-row>\n\n                  <v-select\n                    :items=\"propertystatus\"\n                    item-text=\"title\"\n                    item-value=\"value\"\n                    label=\"Status / တိုက်ခန်း အခြေအနေ\"\n                    v-model=\"propertyDetail.property_status\"\n                    required\n                  ></v-select>\n\n                  <v-row>\n                    <v-col cols=\"6\" sm=\"6\">\n                      <v-text-field\n                        label=\"Area\"\n                        v-model=\"propertyDetail.area\"\n                        autocomplete=\"on\"\n                      ></v-text-field>\n                    </v-col>\n\n                    <v-col cols=\"6\" sm=\"6\">\n                      <v-select\n                        :items=\"areatypes\"\n                        item-text=\"title\"\n                        item-value=\"value\"\n                        v-model=\"propertyDetail.area_type\"\n                        required\n                      ></v-select>\n                    </v-col>\n                  </v-row>\n\n                  <v-select\n                    :items=\"rooms\"\n                    item-text=\"title\"\n                    item-value=\"value\"\n                    label=\"Room\"\n                    v-model=\"propertyDetail.rooms\"\n                  ></v-select>\n\n                  <v-select\n                    :items=\"rooms\"\n                    item-text=\"title\"\n                    item-value=\"value\"\n                    label=\"Bathroom\"\n                    v-model=\"propertyDetail.bathroom\"\n                  ></v-select>\n\n                  <v-select\n                    :items=\"rooms\"\n                    item-text=\"title\"\n                    item-value=\"value\"\n                    label=\"Bedroom\"\n                    v-model=\"propertyDetail.bead_room\"\n                  ></v-select>\n\n                  <v-select\n                    :items=\"rooms\"\n                    item-text=\"title\"\n                    item-value=\"value\"\n                    label=\"Master Bedroom\"\n                    v-model=\"propertyDetail.masterbathroom\"\n                  ></v-select>\n\n                  <v-select\n                    :items=\"rooms\"\n                    item-text=\"title\"\n                    item-value=\"value\"\n                    label=\"Living Room\"\n                    v-model=\"propertyDetail.living_room\"\n                  ></v-select>\n\n                  <v-select\n                    :items=\"floors\"\n                    item-text=\"title\"\n                    item-value=\"value\"\n                    label=\"အလွှာ\"\n                    v-model=\"propertyDetail.floor\"\n                  ></v-select>\n\n                  <v-text-field\n                    label=\"တည်ဆောက်သည့်နှစ် / တိုက်သက်တမ်း\"\n                    v-model=\"propertyDetail.year_built\"\n                    autocomplete=\"on\"\n                  ></v-text-field>\n\n                  <v-select\n                    :items=\"regions\"\n                    :item-text=\"'region_mm'\"\n                    :item-value=\"'region_id'\"\n                    v-model=\"propertyDetail.regions_id\"\n                    label=\"Region/State\"\n                    required\n                  >\n                  </v-select>\n\n                  <v-select\n                    :items=\"townships\"\n                    :item-text=\"'townships_mm'\"\n                    :item-value=\"'township_id'\"\n                    v-model=\"propertyDetail.township_id\"\n                    label=\"Township\"\n                    required\n                  >\n                  </v-select>\n\n                  <v-text-field\n                    label=\"Contact Number\"\n                    v-model=\"propertyDetail.contact_number\"\n                    autocomplete=\"on\"\n                    required\n                  ></v-text-field>\n\n                  <v-radio-group v-model=\"bankinstallment\" row>\n                    <span>ဘဏ်အရစ်ကျ &nbsp;&nbsp;</span>\n                    <v-radio label=\"No\" value=\"No\"></v-radio>\n                    <v-radio label=\"Yes\" value=\"Yes\"></v-radio>\n                  </v-radio-group>\n\n                  <v-textarea\n                    name=\"input-7-1\"\n                    label=\"Description\"\n                    v-model=\"propertyDetail.description_mm\"\n                    required\n                  ></v-textarea>\n\n                  <v-textarea\n                    name=\"input-7-1\"\n                    label=\"Description Eng\"\n                    v-model=\"propertyDetail.description_eng\"\n                  ></v-textarea>\n                </div>\n              </v-card-text>\n              <v-card-actions>\n                <v-btn type=\"submit\" block color=\"warning\" dark>\n                  ကြော်ငြာတင်မည်\n                </v-btn>\n              </v-card-actions>\n            </v-card>\n          </form>\n        </v-tab-item>\n      </v-tabs-items>\n    </v-card>\n  </div>\n</template>\n<script>\nimport { HTTP } from \"@/use/http-common\";\n\nexport default {\n  name: \"Adedit\",\n  props: [\"id\"],\n  data() {\n    return {\n      messages: \"\",\n      success: \"\",\n      tab: null,\n\n      propertyDetail: {},\n\n      types: [\n        \"ရောင်းရန်အိမ်ခြံမြေများ\",\n        \"ငှားရန်အိမ်ခြံမြေများ\",\n        \"အိမ်ခြံမြေသစ်များ\",\n      ],\n\n      currency: [\n        {\n          name: \"MMK(Lakhs)\",\n          value: \"MMK(Lakhs)\",\n        },\n        {\n          name: \"USD\",\n          value: \"USD\",\n        },\n      ],\n\n      propertystatus: [\n        {\n          title: \"အလုံးစုံ ပြင်ဆင်ပြီး\",\n          value: \"Fully Furnished\",\n        },\n        {\n          title: \"မပြင်ဆင်ရသေး\",\n          value: \"မပြင်ဆင်ရသေး\",\n        },\n        {\n          title: \"တစ်စိတ်တစ်ပိုင်း ပြင်ဆင်ပြီး\",\n          value: \"တစ်စိတ်တစ်ပိုင်း ပြင်ဆင်ပြီး\",\n        },\n        {\n          title: \"ခန်းမ\",\n          value: \"Hall\",\n        },\n      ],\n\n      areatypes: [\n        {\n          title: \"Sqft\",\n          value: \"Sqft\",\n        },\n        {\n          title: \"Acre\",\n          value: \"Acre\",\n        },\n      ],\n\n      rooms: [\n        {\n          title: \"မပါ\",\n          value: \"0\",\n        },\n        {\n          title: \"1 Room\",\n          value: \"1\",\n        },\n        {\n          title: \"2 Room\",\n          value: \"2\",\n        },\n        {\n          title: \"3 Room\",\n          value: \"3\",\n        },\n        {\n          title: \"4 Room\",\n          value: \"4\",\n        },\n        {\n          title: \"5 Room\",\n          value: \"5\",\n        },\n        {\n          title