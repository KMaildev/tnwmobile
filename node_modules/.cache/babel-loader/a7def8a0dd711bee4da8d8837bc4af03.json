AAA;;AACA,kBAAA,KAAA,CAAA,SAAA,GAAA,QAAA,CAAA,IAAA,CAAA,SAAA;AACA,kBAAA,KAAA,CAAA,MAAA,GAAA,KAAA;AACA,iBALA,WAMA,UAAA,CAAA,EAAA;AACA,kBAAA,KAAA,CAAA,MAAA,GAAA,KAAA;AACA,iBARA;;AADA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAUA,KAXA;AAaA,IAAA,oBAbA,gCAaA,SAbA,EAaA;AACA,UAAA,CAAA,SAAA,EAAA;AACA;AACA;;AACA,WAAA,IAAA;AACA,WAAA,KAAA;AACA;AAnBA,GApBA;AA0CA,EAAA,OA1CA,qBA0CA;AACA,SAAA,IAAA,GAAA,CAAA;AACA,SAAA,KAAA;AACA,GA7CA;AA+CA,EAAA,OA/CA,qBA+CA;AACA,QAAA,QAAA,GAAA,YAAA,CAAA,OAAA,CAAA,UAAA,CAAA;;AACA,QAAA,QAAA,EAAA;AACA,WAAA,WAAA,GAAA,IAAA;AACA,KAFA,MAEA;AACA,WAAA,WAAA,GAAA,KAAA;AACA;AACA;AAtDA,CAAA","sourcesContent":["<template>\n  <div>\n    <Loader v-show=\"isLoad\" />\n    <v-card\n      class=\"mx-auto my-2\"\n      max-width=\"344\"\n      outlined\n      v-show=\"!isLoad\"\n      v-for=\"(property, index) in propertyies\"\n      :key=\"index\"\n    >\n      <v-list-item three-line>\n        <v-list-item-content>\n          <v-list-item-title style=\"font-size: 15px\">\n            {{ property.title }}\n          </v-list-item-title>\n          <v-list-item-subtitle style=\"font-size: 13px; color: #af742e\">\n            <v-icon style=\"font-size: 17px\">mdi-map-marker</v-icon>\n            {{ property.region_mm }} | {{ property.townships_mm }}\n          </v-list-item-subtitle>\n        </v-list-item-content>\n      </v-list-item>\n\n      <v-expand-transition>\n        <div class=\"mb-6\">\n          <v-card-text class=\"pt-0 pb-4\">\n            <v-icon style=\"font-size: 17px\">mdi-office-building-outline</v-icon>\n            {{ property.property_type_mm }}\n          </v-card-text>\n\n          <v-card-text\n            class=\"pt-0 pb-3\"\n            style=\"font-size: 15px; font-weight: bold; color: blue\"\n          >\n            <span v-if=\"property.currency == 'MMK(Lakhs)'\"> သိန်း </span>\n            <span v-else> USD </span>\n            {{ property.budget_from }}\n            မှ\n\n            <span v-if=\"property.currency == 'MMK(Lakhs)'\"> သိန်း </span>\n            <span v-else> USD </span>\n            {{ property.budget_to }}\n            အတွင်း\n          </v-card-text>\n        </div>\n      </v-expand-transition>\n      <v-footer absolute class=\"pa-1\" color=\"transparent\">\n        <v-btn\n          block\n          small\n          color=\"warning\"\n          :to=\"{\n            name: 'WantedDetail',\n            params: { id: property.w_id },\n          }\"\n        >\n          {{ setLanguage == true ? \"Detail\" : \"အသေးစိတ်ကြည့်ရန်\" }}\n        </v-btn>\n      </v-footer>\n    </v-card>\n    <div\n      v-if=\"propertyies.length\"\n      v-observe-visibility=\"handleScrollToButton\"\n    ></div>\n  </div>\n</template>\n<script>\nimport { HTTP } from \"@/use/http-common\";\nimport Loader from \"@/components/Loader\";\nimport { IMAGE_URL, LogoNotFound, APP_BASE_URL } from \"@/use/image-url\";\n\nexport default {\n  name: \"WantToBuy\",\n  components: {\n    Loader,\n  },\n\n  data() {\n    return {\n      tab: null,\n      setLanguage: false,\n      isLoad: false,\n      propertyies: [],\n      tatnaywon: {},\n      page: 0,\n      IMAGE_URL: IMAGE_URL,\n      LogoNotFound: LogoNotFound,\n      APP_BASE_URL: APP_BASE_URL,\n    };\n  },\n\n  methods: {\n    async fetch() {\n      HTTP.get(`wanted_listing/index/want_to_buy/${this.page}`)\n        .then((response) => {\n          this.propertyies.push(...response.data.data);\n          this.tatnaywon = response.data.tatnaywon;\n          this.isLoad = false;\n        })\n        .catch((e) => {\n          this.isLoad = false;\n        });\n    },\n\n    handleScrollToButton(isVisible) {\n      if (!isVisible) {\n        return;\n      }\n      this.page++;\n      this.fetch();\n    },\n  },\n\n  mounted() {\n    this.page = 0;\n    this.fetch();\n  },\n\n  created() {\n    let language = localStorage.getItem(\"language\");\n    if (language) {\n      this.setLanguage = true;\n    } else {\n      this.setLanguage = false;\n    }\n  },\n};\n</script>\n"],"sourceRoot":"src/components"}]}