IAAA,KADA,iBACA,SADA,EACA;AACA,WAAA,cAAA,GAAA,SAAA;;AACA,UAAA,SAAA,EAAA;AACA,YAAA,SAAA,KAAA,OAAA,EAAA;AACA,eAAA,OAAA,CAAA,EAAA,CAAA,CAAA,CAAA;AACA;AACA;AACA,KARA;AAUA,IAAA,aAVA,2BAUA;AAAA;;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AACA,gBAAA,MADA,GACA,KAAA,CAAA,gBAAA,CAAA,OADA;AAEA,gBAAA,IAAA,CAAA,GAAA,gCAAA,MAAA,GACA,IADA,CACA,UAAA,QAAA,EAAA;AACA,kBAAA,KAAA,CAAA,kBAAA,GAAA,QAAA,CAAA,IAAA,CAAA,YAAA;AACA,iBAHA,WAIA,UAAA,CAAA,EAAA;AACA,kBAAA,KAAA,CAAA,kBAAA,GAAA,CAAA;AACA,iBANA;;AAFA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AASA,KAnBA;AAqBA,IAAA,yBArBA,uCAqBA;AAAA;;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AACA,gBAAA,MADA,GACA,MAAA,CAAA,gBAAA,CAAA,OADA;AAEA,gBAAA,IAAA,CAAA,GAAA,0BAAA,MAAA,GACA,IADA,CACA,UAAA,QAAA,EAAA;AACA,kBAAA,MAAA,CAAA,iBAAA,GAAA,QAAA,CAAA,IAAA,CAAA,IAAA;AACA,iBAHA,WAIA,UAAA,CAAA,EAAA;AACA,kBAAA,MAAA,CAAA,iBAAA,GAAA,CAAA;AACA,iBANA;;AAFA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AASA,KA9BA;AAgCA,IAAA,mBAAA,EAAA,6BAAA,EAAA,EAAA;AAAA;;AACA,MAAA,IAAA,CAAA,GAAA,qCAAA,EAAA,GACA,IADA,CACA,UAAA,QAAA,EAAA;AACA,YAAA,QAAA,CAAA,MAAA,KAAA,GAAA,EAAA;AACA,UAAA,MAAA,CAAA,OAAA,GAAA,+BAAA;AACA,UAAA,MAAA,CAAA,KAAA,GAAA,EAAA;AACA;AACA,OANA,WAOA,UAAA,CAAA,EAAA;AACA,QAAA,MAAA,CAAA,KAAA,GAAA,cAAA;AACA,QAAA,MAAA,CAAA,OAAA,GAAA,EAAA;AACA,OAVA;AAWA;AA5CA,GA3BA;AA0EA,EAAA,OA1EA,qBA0EA;AACA,SAAA,aAAA;AACA,SAAA,yBAAA;AACA,GA7EA;AA+EA,EAAA,OA/EA,qBA+EA;AAAA;;AACA,QAAA,YAAA,GAAA,YAAA,CAAA,OAAA,CAAA,cAAA,CAAA;AACA,SAAA,gBAAA,GAAA,IAAA,CAAA,KAAA,CAAA,YAAA,CAAA;AAEA,IAAA,IAAA,CAAA,GAAA,2BAAA,KAAA,MAAA,CAAA,MAAA,CAAA,EAAA,GACA,IADA,CACA,UAAA,QAAA,EAAA;AAAA;;AACA,MAAA,MAAA,CAAA,cAAA,GAAA,QAAA,CAAA,IAAA,CAAA,IAAA;;AACA,+BAAA,MAAA,CAAA,cAAA,EAAA,IAAA,iDAAA,QAAA,CAAA,IAAA,CAAA,KAAA;;AACA,MAAA,MAAA,CAAA,SAAA,GAAA,QAAA,CAAA,IAAA,CAAA,SAAA;AACA,MAAA,MAAA,CAAA,MAAA,GAAA,KAAA;AACA,KANA,WAOA,UAAA,CAAA,EAAA;AACA,MAAA,MAAA,CAAA,MAAA,GAAA,IAAA;AACA,KATA;AAUA;AA7FA,CAAA","sourcesContent":["<template>\n  <div\n    v-touch=\"{\n      left: () => swipe('Left'),\n      right: () => swipe('Right'),\n      up: () => swipe('Up'),\n      down: () => swipe('Down'),\n    }\"\n  >\n    <v-alert dense outlined type=\"success\" v-if=\"success\" style=\"margin: 2px\">\n      {{ success }}\n    </v-alert>\n\n    <v-alert dense outlined type=\"danger\" v-if=\"error\" style=\"margin: 2px\">\n      {{ error }}\n    </v-alert>\n\n    <v-simple-table class=\"mb-2\">\n      <template>\n        <tbody style=\"background-color: #fafafa\">\n          <tr>\n            <td style=\"font-size: 12px\">\n              လစဉ်အထူးကြော်ငြာပြုလုပ်နိုင်သည့် အရေအတွက်\n            </td>\n            <td style=\"font-size: 12px\">\n              {{ no_of_featuredpost }}\n            </td>\n          </tr>\n\n          <tr>\n            <td style=\"font-size: 12px\">အထူးကြော်ငြာ ပြုလုပ်ပြီးသောအရေအတွက်</td>\n            <td style=\"font-size: 12px\">{{ total_featured_ad }}</td>\n          </tr>\n\n          <tr>\n            <td style=\"font-size: 12px\">\n              အထူးကြော်ငြာ ပြုလုပ်ရန် ကျန်ရှိသောအရေအတွက်\n            </td>\n            <td style=\"font-size: 12px\">\n              {{ (PostTotal = +(no_of_featuredpost - total_featured_ad)) }}\n            </td>\n          </tr>\n        </tbody>\n      </template>\n    </v-simple-table>\n\n    <div v-if=\"no_of_featuredpost == 0\">\n      <PackageNoOrder />\n    </div>\n\n    <div v-else-if=\"PostTotal == 0\">\n      <FeaturedAdAlert />\n    </div>\n\n    <div v-else>\n      <v-alert color=\"warning\">\n        <div class=\"\" style=\"color: white; font-weight: bold; font-size: 20px\">\n          ဤအိမ်ခြံမြေအား အထူးကြော်ငြာပြုလုပ်မည်\n        </div>\n        <br />\n        <p style=\"color: white; font-weight: bold\">\n          {{ propertyDetail.title_mm }}\n          <br />\n          ကြော်ငြာနံပါတ် - {{ propertyDetail.ad_number }}\n          <br />\n          စျေးနှုန်း -\n          {{ propertyDetail.price }}\n          {{\n            propertyDetail.price_type == \"MMK(Lakhs)\" ? \"သိန်း(ကျပ်)\" : \"USD\"\n          }}\n        </p>\n      </v-alert>\n\n      <v-btn\n        block\n        color=\"warning\"\n        @click=\"FeaturedPropertyNow($route.params.id)\"\n      >\n        အထူးကြော်ငြာပြုလုပ်မည်\n      </v-btn>\n    </div>\n  </div>\n</template>\n\n<script>\nimport { HTTP } from \"@/use/http-common\";\nimport Loader from \"@/components/Loader\";\nimport PackageNoOrder from \"@/components/PackageNoOrder\";\nimport FeaturedAdAlert from \"@/components/FeaturedAdAlert\";\n\nimport { IMAGE_URL, APP_BASE_URL } from \"@/use/image-url\";\n\nexport default {\n  name: \"FeaturedProperty\",\n\n  components: {\n    Loader,\n    PackageNoOrder,\n    FeaturedAdAlert,\n  },\n\n  data() {\n    return {\n      isLoad: true,\n      tnwuserdatastore: {},\n      no_of_featuredpost: 0,\n      success: \"\",\n      error: \"\",\n\n      propertyDetail: {},\n      propertyPhotos: [],\n      tatnaywon: {},\n      IMAGE_URL: IMAGE_URL,\n      APP_BASE_URL: APP_BASE_URL,\n\n      total_featured_ad: 0,\n    };\n  },\n\n  methods: {\n    swipe(direction) {\n      this.swipeDirection = direction;\n      if (direction) {\n        if (direction === \"Right\") {\n          this.$router.go(-1);\n        }\n      }\n    },\n\n    async fetch_package() {\n      var userId = this.tnwuserdatastore.user_id;\n      HTTP.get(`package/featuredpost/${userId}`)\n        .then((response) => {\n          this.no_of_featuredpost = response.data.featuredpost;\n        })\n        .catch((e) => {\n          this.no_of_featuredpost = 0;\n        });\n    },\n\n    async total_featured_ad_counter() {\n      var userId = this.tnwuserdatastore.user_id;\n      HTTP.get(`featured/index/${userId}`)\n        .then((response) => {\n          this.total_featured_ad = response.data.data;\n        })\n        .catch((e) => {\n          this.total_featured_ad = 0;\n        });\n    },\n\n    FeaturedPropertyNow: function (id) {\n      HTTP.get(`property/featuredproperty/${id}`)\n        .then((response) => {\n          if (response.status === 200) {\n            this.success = \"အထူးကြော်ငြာပြုလုပ် ပြီးပါပြီ\";\n            this.error = \"\";\n          }\n        })\n        .catch((e) => {\n          this.error = \"မအောင်မြင်ပါ\";\n          this.success = \"\";\n        });\n    },\n  },\n\n  mounted() {\n    this.fetch_package();\n    this.total_featured_ad_counter();\n  },\n\n  created() {\n    let tnwuserstore = localStorage.getItem(\"tnwuserstore\");\n    this.tnwuserdatastore = JSON.parse(tnwuserstore);\n\n    HTTP.get(`property/detail/${this.$route.params.id}`)\n      .then((response) => {\n        this.propertyDetail = response.data.data;\n        this.propertyPhotos.push(...response.data.photo);\n        this.tatnaywon = response.data.tatnaywon;\n        this.isLoad = false;\n      })\n      .catch((e) => {\n        this.isLoad = true;\n      });\n  },\n};\n</script>"],"sourceRoot":"src/views/featured"}]}