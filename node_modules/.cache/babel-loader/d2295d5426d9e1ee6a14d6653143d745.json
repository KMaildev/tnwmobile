AACA,MAAA,MAAA,EAAA,KADA;AAEA,MAAA,WAAA,EAAA,EAFA;AAGA,MAAA,SAAA,EAAA,EAHA;AAIA,MAAA,gBAAA,EAAA,EAJA;AAKA,MAAA,IAAA,EAAA,CALA;AAMA,MAAA,SAAA,EAAA,SANA;AAOA,MAAA,aAAA,EAAA,aAPA;AAQA,MAAA,YAAA,EAAA,YARA;AASA,MAAA,YAAA,EAAA,YATA;AAUA,MAAA,OAAA,EAAA;AAVA,KAAA;AAYA,GArBA;AAuBA,EAAA,OAAA,EAAA;AACA,IAAA,KADA,mBACA;AAAA;;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AACA,gBAAA,MADA,GACA,KAAA,CAAA,gBAAA,CAAA,OADA;AAEA,gBAAA,IAAA,CAAA,GAAA,2BAAA,MAAA,cAAA,KAAA,CAAA,IAAA,GACA,IADA,CACA,UAAA,QAAA,EAAA;AAAA;;AACA,uCAAA,KAAA,CAAA,WAAA,EAAA,IAAA,6CAAA,QAAA,CAAA,IAAA,CAAA,IAAA;;AACA,kBAAA,KAAA,CAAA,SAAA,GAAA,QAAA,CAAA,IAAA,CAAA,SAAA;AACA,kBAAA,KAAA,CAAA,MAAA,GAAA,KAAA;AACA,iBALA,WAMA,UAAA,CAAA,EAAA;AACA,kBAAA,KAAA,CAAA,MAAA,GAAA,KAAA;AACA,iBARA;;AAFA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAWA,KAZA;AAcA,IAAA,eAAA,EAAA,yBAAA,EAAA,EAAA;AAAA;;AACA,MAAA,IAAA,CAAA,GAAA,qCAAA,EAAA,GACA,IADA,CACA,UAAA,QAAA,EAAA;AACA,YAAA,QAAA,CAAA,MAAA,KAAA,GAAA,EAAA;AACA,UAAA,MAAA,CAAA,OAAA,GAAA,oCAAA;AACA;AACA,OALA,WAMA,UAAA,CAAA,EAAA;AACA,QAAA,MAAA,CAAA,KAAA,GAAA,cAAA;AACA,OARA;AASA,KAxBA;AA0BA,IAAA,KA1BA,iBA0BA,SA1BA,EA0BA;AACA,WAAA,cAAA,GAAA,SAAA;;AACA,UAAA,SAAA,EAAA;AACA,YAAA,SAAA,KAAA,OAAA,EAAA;AACA,eAAA,OAAA,CAAA,EAAA,CAAA,CAAA,CAAA;AACA;AACA;AACA,KAjCA;AAmCA,IAAA,oBAnCA,gCAmCA,SAnCA,EAmCA;AACA,UAAA,CAAA,SAAA,EAAA;AACA;AACA;;AACA,WAAA,IAAA;AACA,WAAA,KAAA;AACA,KAzCA;AA2CA,IAAA,KA3CA,iBA2CA,EA3CA,EA2CA;AACA,MAAA,SAAA,CACA,KADA,CACA;AACA,QAAA,KAAA,EAAA,uBADA;AAEA,QAAA,GAAA,EAAA,YAAA,GAAA,uBAAA,GAAA;AAFA,OADA,EAKA,IALA,CAKA,UAAA,CAAA;AAAA,eAAA,OAAA,CAAA,GAAA,CAAA,uBAAA,CAAA;AAAA,OALA,WAMA,UAAA,KAAA;AAAA,eACA,OAAA,CAAA,GAAA,CAAA,sCAAA,EAAA,KAAA,CADA;AAAA,OANA;AASA;AArDA,GAvBA;AA+EA,EAAA,KAAA,EAAA;AACA,IAAA,MADA,kBACA,EADA,EACA,IADA,EACA;AACA,UAAA,SAAA,GAAA,IAAA,CAAA,MAAA,CAAA,YAAA;AACA,UAAA,YAAA,GAAA,KAAA,MAAA,CAAA,MAAA,CAAA,YAAA;;AACA,UAAA,SAAA,IAAA,YAAA,EAAA;AACA,aAAA,IAAA,GAAA,CAAA;AACA,aAAA,KAAA;AACA;AACA;AARA,GA/EA;AA0FA,EAAA,OA1FA,qBA0FA;AACA,SAAA,IAAA,GAAA,CAAA;AACA,SAAA,KAAA;AACA,GA7FA;AA+FA,EAAA,OA/FA,qBA+FA;AACA,QAAA,YAAA,GAAA,YAAA,CAAA,OAAA,CAAA,cAAA,CAAA;AACA,SAAA,gBAAA,GAAA,IAAA,CAAA,KAAA,CAAA,YAAA,CAAA;AACA;AAlGA,CAAA","sourcesContent":["<template>\n  <div\n    v-touch=\"{\n      left: () => swipe('Left'),\n      right: () => swipe('Right'),\n      up: () => swipe('Up'),\n      down: () => swipe('Down'),\n    }\"\n  >\n    <div v-if=\"propertyies.length == 0\" class=\"text-center\">\n      You've no posts.\n    </div>\n\n    <v-alert dense outlined type=\"success\" v-if=\"success\" style=\"margin: 2px\">\n      {{ success }}\n    </v-alert>\n\n    <Loader v-show=\"isLoad\" />\n    <v-card\n      v-show=\"!isLoad\"\n      class=\"mx-auto my-5\"\n      v-for=\"(property, index) in propertyies\"\n      :key=\"index\"\n    >\n      <div>\n        <div class=\"card\">\n          <div class=\"ovelay\"></div>\n          <header class=\"user\">\n            <!-- Agent Logo -->\n            <img\n              v-if=\"property.user_info_id == 0\"\n              :src=\"TatNayWonLogo\"\n              alt=\"Logo\"\n            />\n            <img v-else :src=\"IMAGE_URL + property.logo\" alt=\"Logo\" />\n            <!-- Agent Logo -->\n            <div class=\"user-info\">\n              <h2 class=\"user-info-name\" style=\"color: black\">\n                <span v-if=\"property.user_info_id == 0\">\n                  TatNayWonProperty.com\n                </span>\n                <router-link\n                  style=\"text-decoration: none\"\n                  v-else\n                  :to=\"{\n                    name: 'agentDetail',\n                    params: { id: property.user_id },\n                  }\"\n                >\n                  {{ property.company_name }}\n                </router-link>\n              </h2>\n              <p class=\"user-info-time\" style=\"color: black\">\n                {{ property.post_upload_date }}\n              </p>\n            </div>\n            <v-spacer></v-spacer>\n            <v-bottom-sheet inset>\n              <template v-slot:activator=\"{ on, attrs }\">\n                <v-btn icon color=\"pink\" v-bind=\"attrs\" v-on=\"on\">\n                  <v-icon>mdi-dots-vertical</v-icon>\n                </v-btn>\n              </template>\n              <v-sheet height=\"auto\">\n                <v-list class=\"transparent\">\n                  <v-list-item style=\"min-height: 30px\">\n                    <v-list-item-title style=\"font-size: 13px\">\n                      ကြော်ငြာနံပါတ်\n                    </v-list-item-title>\n                    <v-list-item-subtitle\n                      class=\"text-right\"\n                      style=\"font-size: 13px; color: #0f4c8c; font-weight: bold\"\n                    >\n                      {{ property.ad_number }}\n                    </v-list-item-subtitle>\n                  </v-list-item>\n\n                  <v-list-item style=\"min-height: 30px\">\n                    <v-list-item-title style=\"font-size: 13px\">\n                      စျေးနှုန်း\n                    </v-list-item-title>\n                    <v-list-item-subtitle\n                      class=\"text-right\"\n                      style=\"font-size: 13px; color: #0f4c8c; font-weight: bold\"\n                    >\n                      {{ property.price }}\n                      {{\n                        property.price_type == \"MMK(Lakhs)\"\n                          ? \"သိန်း(ကျပ်)\"\n                          : \"USD\"\n                      }}\n                    </v-list-item-subtitle>\n                  </v-list-item>\n\n                  <v-list-item style=\"min-height: 30px\">\n                    <v-list-item-title style=\"font-size: 13px\">\n                      အကျယ်အဝန်း\n                    </v-list-item-title>\n                    <v-list-item-subtitle\n                      class=\"text-right\"\n                      style=\"font-size: 13px; color: #0f4c8c; font-weight: bold\"\n                    >\n                      {{ property.area }}\n                      {{ property.area_type }}\n                    </v-list-item-subtitle>\n                  </v-list-item>\n\n                  <v-list-item style=\"min-height: 30px\">\n                    <v-list-item-title style=\"font-size: 13px\">\n                      အမျိုးအစား\n                    </v-list-item-title>\n                    <v-list-item-subtitle\n                      class=\"text-right\"\n                      style=\"font-size: 13px; color: #0f4c8c; font-weight: bold\"\n                    >\n                      {{ property.property_type_mm }}\n                    </v-list-item-subtitle>\n                  </v-list-item>\n\n                  <v-list-item style=\"min-height: 30px\">\n                    <v-list-item-title style=\"font-size: 13px\">\n                      တိုင်း၊ ပြည်နယ်\n                    </v-list-item-title>\n                    <v-list-item-subtitle\n                      class=\"text-right\"\n                      style=\"font-size: 13px; color: #0f4c8c; font-weight: bold\"\n                    >\n                      {{ property.region_mm }}\n                    </v-list-item-subtitle>\n                  </v-list-item>\n\n                  <v-list-item style=\"min-height: 30px\">\n                    <v-list-item-title style=\"font-size: 13px\">\n                      မြို့နယ်\n                    </v-list-item-title>\n                    <v-list-item-subtitle\n                      class=\"text-right\"\n                      style=\"font-size: 13px; color: #0f4c8c; font-weight: bold\"\n                    >\n                      {{ property.townships_mm }}\n                    </v-list-item-subtitle>\n                  </v-list-item>\n\n                  <v-list-item>\n                    <v-btn\n                      :to=\"{\n                        name: 'MyPropertyDetail',\n                        params: { id: property.sale_id },\n                      }\"\n                      small\n                      color=\"warning\"\n                      block\n                    >\n                      <v-icon left small>mdi-eye</v-icon>\n                      အသေးစိတ်ကြည့်ရန်\n                    </v-btn>\n                  </v-list-item>\n                </v-list>\n              </v-sheet>\n            </v-bottom-sheet>\n          </header>\n\n          <router-link\n            :to=\"{\n              name: 'MyPropertyDetail',\n              params: { id: property.sale_id },\n            }\"\n            style=\"font-size: 12px; color: #0060b6; text-decoration: none\"\n          >\n            {{ property.title_mm }}\n          </router-link>\n\n          <img\n            v-if=\"property.photo\"\n            :src=\"IMAGE_URL + property.photo\"\n            alt=\"Property\"\n            style=\"\n              width: 100%;\n              height: 160px;\n              object-fit: cover;\n              object-position: top;\n            \"\n          />\n        </div>\n\n        <v-row align=\"center\" justify=\"space-around\" class=\"my-1\">\n          <div class=\"text-center\" style=\"background-color: #fcf9f9\">\n            <v-chip style=\"margin: 2px\" color=\"primary\" label small>\n              <v-icon small left class=\"material-icons\">\n                bedroom_parent\n              </v-icon>\n              {{ property.rooms == \"\" ? \"0\" : property.rooms }}\n            </v-chip>\n\n            <v-chip style=\"margin: 2px\" color=\"success\" label small>\n              <v-icon small left class=\"material-icons\"> bathtub </v-icon>\n              {{ property.bathroom == \"\" ? \"0\" : property.bathroom }}\n            </v-chip>\n\n            <v-chip style=\"margin: 2px\" color=\"error\" label small>\n              <v-icon small left class=\"material-icons\"> chair </v-icon>\n              {{ property.living_room == \"\" ? \"0\" : property.living_room }}\n            </v-chip>\n\n            <v-chip style=\"margin: 2px\" color=\"warning\" label small>\n              <v-icon small left class=\"material-icons\">attach_money</v-icon>\n              {{ property.price }} {{ property.price_type }}\n            </v-chip>\n          </div>\n\n          <div class=\"text-center\">\n            <v-card-actions style=\"padding: 5px\">\n              <v-spacer></v-spacer>\n              <v-btn\n                color=\"detailBtn\"\n                x-small\n                class=\"white--text\"\n                @click=\"RemoveFavorites(property.f_id)\"\n              >\n                <v-icon left small>mdi-swap-horizontal</v-icon>\n                အနှစ်သက်ဆုံးစားရင်းမှဖျက်ရန်\n              </v-btn>\n\n              <v-btn\n                :to=\"{\n                  name: 'propertyDetail',\n                  params: { id: property.properties_data_id },\n                }\"\n                color=\"detailBtn\"\n                x-small\n                class=\"white--text\"\n              >\n                <v-icon left small>mdi-eye</v-icon>\n                ကြည့်ရန်\n              </v-btn>\n\n              <v-spacer></v-spacer>\n            </v-card-actions>\n          </div>\n        </v-row>\n      </div>\n    </v-card>\n  </div>\n</template>\n\n<script>\nimport { HTTP } from \"@/use/http-common\";\nimport Loader from \"@/components/Loader\";\nimport PropertyCount from \"@/components/PropertyCount\";\nimport {\n  IMAGE_URL,\n  LogoNotFound,\n  TatNayWonLogo,\n  APP_BASE_URL,\n} from \"@/use/image-url\";\n\nexport default {\n  name: \"Favorites\",\n\n  components: {\n    Loader,\n    PropertyCount,\n  },\n\n  data() {\n    return {\n      isLoad: false,\n      propertyies: [],\n      tatnaywon: {},\n      tnwuserdatastore: {},\n      page: 0,\n      IMAGE_URL: IMAGE_URL,\n      TatNayWonLogo: TatNayWonLogo,\n      LogoNotFound: LogoNotFound,\n      APP_BASE_URL: APP_BASE_URL,\n      success: \"\",\n    };\n  },\n\n  methods: {\n    async fetch() {\n      var userId = this.tnwuserdatastore.user_id;\n      HTTP.get(`favorites/index/${userId}/${this.page}`)\n        .then((response) => {\n          this.propertyies.push(...response.data.data);\n          this.tatnaywon = response.data.tatnaywon;\n          this.isLoad = false;\n        })\n        .catch((e) => {\n          this.isLoad = false;\n        });\n    },\n\n    RemoveFavorites: function (id) {\n      HTTP.get(`favorites/RemoveFavorites/${id}`)\n        .then((response) => {\n          if (response.status === 200) {\n            this.success = \"အနှစ်သက်ဆုံးစားရင်းမှဖျက်ပြီးပါပြီ\";\n          }\n        })\n        .catch((e) => {\n          this.error = \"မအောင်မြင်ပါ\";\n        });\n    },\n\n    swipe(direction) {\n      this.swipeDirection = direction;\n      if (direction) {\n        if (direction === \"Right\") {\n          this.$router.go(-1);\n        }\n      }\n    },\n\n    handleScrollToButton(isVisible) {\n      if (!isVisible) {\n        return;\n      }\n      this.page++;\n      this.fetch();\n    },\n\n    share(id) {\n      navigator\n        .share({\n          title: \"tatnaywonproperty.com\",\n          url: APP_BASE_URL + \"propertydetail/index/\" + id,\n        })\n        .then((_) => console.log(\"Yay, you shared it :)\"))\n        .catch((error) =>\n          console.log(\"Oh noh! You couldn't share it! :'(\\n\", error)\n        );\n    },\n  },\n\n  watch: {\n    $route(to, from) {\n      const fromParam = from.params.propertyType;\n      const currentParam = this.$route.params.propertyType;\n      if (fromParam != currentParam) {\n        this.page = 0;\n        this.fetch();\n      }\n    },\n  },\n\n  mounted() {\n    this.page = 0;\n    this.fetch();\n  },\n\n  created() {\n    let tnwuserstore = localStorage.getItem(\"tnwuserstore\");\n    this.tnwuserdatastore = JSON.parse(tnwuserstore);\n  },\n};\n</script>\n\n<style scoped>\n.card {\n  width: 100%;\n  border-radius: 3px;\n  background: white;\n  overflow: hidden;\n  padding-top: 10px;\n  padding-left: 10px;\n  padding-right: 10px;\n  position: relative;\n}\n.card header.user {\n  display: flex;\n  gap: 12px;\n}\n.card header.user img {\n  height: 40px;\n  width: 40px;\n  border-radius: 50%;\n  object-fit: cover;\n  border: 1px solid #edecea;\n}\n.card header .user-info-name {\n  letter-spacing: 0.4px;\n  font-size: 14px;\n  font-weight: 500;\n  color: #fff;\n}\n.card header .user-info-time {\n  font-size: 12px;\n  color: rgba(255, 255, 255, 0.588);\n}\n</style>\n"],"sourceRoot":"src/views/Favorites"}]}