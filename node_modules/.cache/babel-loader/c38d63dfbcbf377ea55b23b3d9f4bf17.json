AA;AAAA;AAQA,KAnDA;AAqDA,IAAA,iBArDA,+BAqDA;AAAA;;AAAA;AAAA;AAAA;AAAA;AAAA;AACA,gBAAA,IAAA,CAAA,GAAA,eACA,IADA,CACA,UAAA,QAAA,EAAA;AACA,kBAAA,MAAA,CAAA,aAAA,GAAA,QAAA,CAAA,IAAA,CAAA,IAAA;AACA,iBAHA,WAIA,UAAA,CAAA,EAAA;AACA,kBAAA,OAAA,CAAA,GAAA,CAAA,CAAA;AACA,iBANA;;AADA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAQA,KA7DA;AA+DA,IAAA,eA/DA,2BA+DA,OA/DA,EA+DA;AACA,aAAA,OAAA,CAAA,KAAA,GAAA,OAAA,CAAA,KAAA,GAAA,OAAA,CAAA,QAAA,GAAA,GAAA;AACA,KAjEA;AAmEA,IAAA,qBAnEA,iCAmEA,OAnEA,EAmEA;AACA,UAAA,eAAA,GACA,CAAA,OAAA,CAAA,KAAA,GAAA,OAAA,CAAA,KAAA,GAAA,OAAA,CAAA,QAAA,GAAA,GADA;AAEA,UAAA,cAAA,GACA,CAAA,eAAA,GAAA,eAAA,GAAA,OAAA,CAAA,QAAA,GAAA,GADA;AAEA,aAAA,cAAA;AACA,KAzEA;AA2EA,IAAA,WA3EA,uBA2EA,OA3EA,EA2EA;AACA,aAAA,OAAA,CAAA,KAAA,GAAA,OAAA,CAAA,KAAA,GAAA,OAAA,CAAA,kBAAA,GAAA,GAAA;AACA,KA7EA;AA+EA,IAAA,eA/EA,2BA+EA,OA/EA,EA+EA;AACA,UAAA,aAAA,GACA,CAAA,OAAA,CAAA,KAAA,GAAA,OAAA,CAAA,KAAA,GAAA,OAAA,CAAA,kBAAA,GAAA,GADA;AAEA,UAAA,cAAA,GACA,CAAA,aAAA,GAAA,aAAA,GAAA,OAAA,CAAA,kBAAA,GAAA,GADA;AAEA,aAAA,cAAA;AACA;AArFA,GA9BA;AAsHA,EAAA,OAtHA,qBAsHA;AACA,SAAA,iBAAA;AACA,SAAA,iBAAA;AACA,GAzHA;AA2HA,EAAA,OA3HA,qBA2HA;AACA,QAAA,YAAA,GAAA,YAAA,CAAA,OAAA,CAAA,cAAA,CAAA;AACA,SAAA,gBAAA,GAAA,IAAA,CAAA,KAAA,CAAA,YAAA,CAAA;AACA;AA9HA,CAAA","sourcesContent":["<template>\n  <div>\n    <v-list class=\"transparent\">\n      <div style=\"padding: 5px; background-color: #af742e\" class=\"card-panel\">\n        <center style=\"color: white\">\n          {{ packageplan.package_name }}\n        </center>\n      </div>\n      <v-simple-table style=\"background-color: #f2efef\">\n        <template v-slot:default>\n          <tbody>\n            <tr>\n              <td style=\"font-size: 12px\">One Month</td>\n              <td style=\"font-size: 12px\">\n                {{ packageplan.price }}\n              </td>\n            </tr>\n\n            <tr>\n              <td style=\"font-size: 12px\">\n                5% Discount For 3 Months Contract\n                <span style=\"font-size: 12px\"> (Monthly Payment)</span>\n              </td>\n              <td style=\"font-size: 12px\">\n                {{ discountedPrice(packageplan) }}\n              </td>\n            </tr>\n\n            <tr>\n              <td style=\"font-size: 12px\">Additional (Full Payment)</td>\n              <td style=\"font-size: 12px\">\n                {{ DoubleDiscountedPrice(packageplan) }}\n              </td>\n            </tr>\n\n            <tr>\n              <td style=\"font-size: 12px\">\n                10% Discount For 6 Months Contract\n                <span style=\"font-size: 12px\"> (Monthly Payment)</span>\n              </td>\n              <td style=\"font-size: 12px\">\n                {{ sixMonthDis(packageplan) }}\n              </td>\n            </tr>\n\n            <tr>\n              <td style=\"font-size: 12px\">Additional (Full Payment)</td>\n              <td style=\"font-size: 12px\">\n                {{ sixMonthDisFull(packageplan) }}\n              </td>\n            </tr>\n          </tbody>\n        </template>\n      </v-simple-table>\n\n      <v-alert\n        dense\n        outlined\n        type=\"error\"\n        v-if=\"alertMessage\"\n        style=\"margin: 2px\"\n      >\n        {{ alertMessage }}\n      </v-alert>\n\n      <v-alert dense outlined type=\"success\" v-if=\"success\" style=\"margin: 2px\">\n        {{ success }}\n      </v-alert>\n\n      <form @submit.prevent=\"OrderNow\">\n        <v-card flat>\n          <v-card-text>\n            <v-text-field\n              label=\"Phone number or email address\"\n              v-model=\"contact\"\n            ></v-text-field>\n\n            <v-select\n              :items=\"choosepayments\"\n              item-text=\"title\"\n              item-value=\"value\"\n              label=\"Choose Payment\"\n              v-model=\"choose_payment_status\"\n            ></v-select>\n\n            <v-radio-group v-model=\"radios\">\n              <template v-slot:label>\n                <div><strong>Billing Cycle</strong></div>\n              </template>\n              <v-radio value=\"1\">\n                <template v-slot:label>\n                  <div>\n                    1 Month @\n                    <strong class=\"primary--text\">\n                      {{ packageplan.price }}\n                    </strong>\n                  </div>\n                </template>\n              </v-radio>\n              <v-radio value=\"3MonthsContractMonthlyPayment\">\n                <template v-slot:label>\n                  <div>\n                    5% Discount For 3 Months Contract (Monthly Payment) @\n                    <strong class=\"primary--text\">\n                      {{ discountedPrice(packageplan) }}\n                    </strong>\n                  </div>\n                </template>\n              </v-radio>\n\n              <v-radio value=\"3MonthsFullPayment\">\n                <template v-slot:label>\n                  <div>\n                    3 Months Additional (Full Payment)\n                    <strong class=\"primary--text\">\n                      {{ DoubleDiscountedPrice(packageplan) }}\n                    </strong>\n                  </div>\n                </template>\n              </v-radio>\n\n              <v-radio value=\"6MonthsContractMonthlyPayment\">\n                <template v-slot:label>\n                  <div>\n                    10% Discount For 6 Months Contract (Monthly Payment) @\n                    <strong class=\"primary--text\">\n                      {{ sixMonthDis(packageplan) }}\n                    </strong>\n                  </div>\n                </template>\n              </v-radio>\n\n              <v-radio value=\"6MonthsFullPayment\">\n                <template v-slot:label>\n                  <div>\n                    6 Months Additional (Full Payment)\n                    <strong class=\"primary--text\">\n                      {{ sixMonthDisFull(packageplan) }}\n                    </strong>\n                  </div>\n                </template>\n              </v-radio>\n            </v-radio-group>\n\n            <v-radio-group\n              v-model=\"bank_id\"\n              v-if=\"choose_payment_status === 'bank_transfer'\"\n            >\n              <template v-slot:label>\n                <div><strong>Bank Accounts</strong></div>\n              </template>\n              <v-radio\n                v-for=\"(bank_account, index) in bank_accounts\"\n                :key=\"index\"\n                :label=\"`${bank_account.bank_name}@${bank_account.account_number}`\"\n                :value=\"`${bank_account.bank_id}`\"\n              ></v-radio>\n            </v-radio-group>\n\n            <v-file-input\n              v-if=\"choose_payment_status === 'bank_transfer'\"\n              v-model=\"payment_screenshot_file\"\n              placeholder=\"Payment Screenshot\"\n              label=\"Payment Screenshot\"\n              prepend-icon=\"mdi-paperclip\"\n            >\n            </v-file-input>\n\n            <v-btn type=\"submit\" block color=\"warning\" dark> Order Now </v-btn>\n          </v-card-text>\n        </v-card>\n      </form>\n    </v-list>\n  </div>\n</template>\n<script>\nimport { HTTP } from \"@/use/http-common\";\nexport default {\n  name: \"OrderNow\",\n  data() {\n    return {\n      packageplan: {},\n      bank_accounts: [],\n      tnwuserdatastore: {},\n      success: \"\",\n      alertMessage: \"\",\n\n      choosepayments: [\n        {\n          title: \"Contact me\",\n          value: \"contact_me\",\n        },\n\n        {\n          title: \"Bank Transfer\",\n          value: \"bank_transfer\",\n        },\n      ],\n\n      radios: \"1\",\n      contact: \"\",\n      choose_payment_status: \"\",\n      bank_id: \"\",\n      payment_screenshot_file: \"\",\n    };\n  },\n\n  methods: {\n    async OrderNow() {\n      let userId = this.tnwuserdatastore.user_id;\n      let package_id = this.$route.params.id;\n\n      let OneMonthlyPrice = this.packageplan.price;\n\n      let ThreeMonthlyPrice = this.discountedPrice(this.packageplan);\n      let ThreeFullPrice = this.DoubleDiscountedPrice(this.packageplan);\n\n      let SixMonthlyPrice = this.sixMonthDis(this.packageplan);\n      let SixFullPrice = this.sixMonthDisFull(this.packageplan);\n\n      var formData = new FormData();\n      formData.append(\"file\", this.payment_screenshot_file);\n      formData.append(\"contactstatus\", this.contact);\n      formData.append(\"choose_payment_status\", this.choose_payment_status);\n      formData.append(\"BillingCycle\", this.radios);\n      formData.append(\"OneMonthlyPrice\", OneMonthlyPrice);\n      formData.append(\"ThreeMonthlyPrice\", ThreeMonthlyPrice);\n      formData.append(\"ThreeFullPrice\", ThreeFullPrice);\n      formData.append(\"SixMonthlyPrice\", SixMonthlyPrice);\n      formData.append(\"SixFullPrice\", SixFullPrice);\n      formData.append(\"user_id\", userId);\n      formData.append(\"bank_id\", this.bank_id);\n      formData.append(\"package_id\", package_id);\n      HTTP.post(`package/ordernowdemo`, formData, {\n        header: {\n          \"Content-Type\": \"multipart/form-data\",\n        },\n      })\n        .then((response) => {\n          if (response.status === 200) {\n            this.success = \"လုပ်ဆောင်ချက် အောင်မြင်ပါသည်\";\n            this.alertMessage = \"\";\n          }\n        })\n        .catch((e) => {\n          this.alertMessage = \"အချက်အလက်များကို ထည့်သွင်းပါ။\";\n          this.success = \"\";\n        });\n    },\n\n    async fetch_packageplan() {\n      HTTP.get(`package/packagedetail/${this.$route.params.id}`)\n        .then((response) => {\n          this.packageplan = response.data.data;\n        })\n        .catch((e) => {\n          console.log(e);\n        });\n    },\n\n    async fetch_bankaccount() {\n      HTTP.get(`bank/index`)\n        .then((response) => {\n          this.bank_accounts = response.data.data;\n        })\n        .catch((e) => {\n          console.log(e);\n        });\n    },\n\n    discountedPrice(product) {\n      return product.price - (product.price * product.discount) / 100;\n    },\n\n    DoubleDiscountedPrice(product) {\n      var threeMonthValue =\n        +product.price - (product.price * product.discount) / 100;\n      var threeMonthFull =\n        +threeMonthValue - (threeMonthValue * product.discount) / 100;\n      return threeMonthFull;\n    },\n\n    sixMonthDis(product) {\n      return product.price - (product.price * product.six_month_discount) / 100;\n    },\n\n    sixMonthDisFull(product) {\n      var sixMonthValue =\n        +product.price - (product.price * product.six_month_discount) / 100;\n      var threeMonthFull =\n        +sixMonthValue - (sixMonthValue * product.six_month_discount) / 100;\n      return threeMonthFull;\n    },\n  },\n\n  mounted() {\n    this.fetch_packageplan();\n    this.fetch_bankaccount();\n  },\n\n  created() {\n    let tnwuserstore = localStorage.getItem(\"tnwuserstore\");\n    this.tnwuserdatastore = JSON.parse(tnwuserstore);\n  },\n};\n</script>\n"],"sourceRoot":"src/views/Package"}]}