NA;AAQA,MAAA,cAAA,EAAA,EARA;AASA,MAAA,cAAA,EAAA,EATA;AAUA,MAAA,SAAA,EAAA;AAVA,KAAA;AAYA,GAtBA;AAwBA,EAAA,OAAA,EAAA;AACA,IAAA,KADA,iBACA,SADA,EACA;AACA,WAAA,cAAA,GAAA,SAAA;;AACA,UAAA,SAAA,EAAA;AACA,YAAA,SAAA,KAAA,OAAA,EAAA;AACA,eAAA,OAAA,CAAA,EAAA,CAAA,CAAA,CAAA;AACA;AACA;AACA,KARA;AAUA,IAAA,aAVA,2BAUA;AAAA;;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AACA,gBAAA,MADA,GACA,KAAA,CAAA,gBAAA,CAAA,OADA;AAEA,gBAAA,IAAA,CAAA,GAAA,2CAAA,MAAA,GACA,IADA,CACA,UAAA,QAAA,EAAA;AACA,kBAAA,KAAA,CAAA,gBAAA,GAAA,QAAA,CAAA,IAAA,CAAA,uBAAA;AACA,iBAHA,WAIA,UAAA,CAAA,EAAA;AACA,kBAAA,KAAA,CAAA,gBAAA,GAAA,CAAA;AACA,iBANA;;AAFA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AASA,KAnBA;AAqBA,IAAA,wBArBA,sCAqBA;AAAA;;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AACA,gBAAA,MADA,GACA,MAAA,CAAA,gBAAA,CAAA,OADA;AAEA,gBAAA,IAAA,CAAA,GAAA,2BAAA,MAAA,GACA,IADA,CACA,UAAA,QAAA,EAAA;AACA,kBAAA,MAAA,CAAA,mBAAA,GAAA,QAAA,CAAA,IAAA,CAAA,wBAAA;AACA,iBAHA,WAIA,UAAA,CAAA,EAAA;AACA,kBAAA,MAAA,CAAA,mBAAA,GAAA,CAAA;AACA,iBANA;;AAFA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AASA,KA9BA;AAgCA,IAAA,YAAA,EAAA,sBAAA,EAAA,EAAA;AAAA;;AACA,UAAA,MAAA,GAAA,KAAA,gBAAA,CAAA,OAAA;AACA,MAAA,IAAA,CAAA,GAAA,8BAAA,EAAA,cAAA,MAAA,GACA,IADA,CACA,UAAA,QAAA,EAAA;AACA,YAAA,QAAA,CAAA,MAAA,KAAA,GAAA,EAAA;AACA,UAAA,MAAA,CAAA,OAAA,GAAA,sBAAA;AACA,UAAA,MAAA,CAAA,KAAA,GAAA,EAAA;;AAEA,UAAA,MAAA,CAAA,aAAA;;AACA,UAAA,MAAA,CAAA,wBAAA;AACA;AACA,OATA,WAUA,UAAA,CAAA,EAAA;AACA,QAAA,MAAA,CAAA,KAAA,GAAA,cAAA;AACA,QAAA,MAAA,CAAA,OAAA,GAAA,EAAA;AACA,OAbA;AAcA;AAhDA,GAxBA;AA2EA,EAAA,OA3EA,qBA2EA;AACA,SAAA,aAAA;AACA,SAAA,wBAAA;AACA,GA9EA;AAgFA,EAAA,OAhFA,qBAgFA;AAAA;;AACA,QAAA,YAAA,GAAA,YAAA,CAAA,OAAA,CAAA,cAAA,CAAA;AACA,SAAA,gBAAA,GAAA,IAAA,CAAA,KAAA,CAAA,YAAA,CAAA;AAEA,IAAA,IAAA,CAAA,GAAA,2BAAA,KAAA,MAAA,CAAA,MAAA,CAAA,EAAA,GACA,IADA,CACA,UAAA,QAAA,EAAA;AAAA;;AACA,MAAA,MAAA,CAAA,cAAA,GAAA,QAAA,CAAA,IAAA,CAAA,IAAA;;AACA,+BAAA,MAAA,CAAA,cAAA,EAAA,IAAA,iDAAA,QAAA,CAAA,IAAA,CAAA,KAAA;;AACA,MAAA,MAAA,CAAA,SAAA,GAAA,QAAA,CAAA,IAAA,CAAA,SAAA;AACA,MAAA,MAAA,CAAA,MAAA,GAAA,KAAA;AACA,KANA,WAOA,UAAA,CAAA,EAAA;AACA,MAAA,MAAA,CAAA,MAAA,GAAA,IAAA;AACA,KATA;AAUA;AA9FA,CAAA","sourcesContent":["<template>\n  <div\n    v-touch=\"{\n      left: () => swipe('Left'),\n      right: () => swipe('Right'),\n      up: () => swipe('Up'),\n      down: () => swipe('Down'),\n    }\"\n  >\n    <v-alert dense outlined type=\"success\" v-if=\"success\" style=\"margin: 2px\">\n      {{ success }}\n    </v-alert>\n\n    <v-alert dense outlined type=\"danger\" v-if=\"error\" style=\"margin: 2px\">\n      {{ error }}\n    </v-alert>\n\n    <v-simple-table class=\"mb-2\">\n      <template>\n        <tbody style=\"background-color: #fafafa\">\n          <tr>\n            <td style=\"font-size: 12px\">\n              နေ့စဉ် Refresh ပြုလုပ်နိုင်သည့်အရေအတွက်\n            </td>\n            <td style=\"font-size: 12px\">\n              {{ no_of_refresh_ad }}\n            </td>\n          </tr>\n\n          <tr>\n            <td style=\"font-size: 12px\">ယနေ့ Refresh ပြုလုပ်ပြီးသောအရေအတွက်</td>\n            <td style=\"font-size: 12px\">{{ today_no_of_refresh }}</td>\n          </tr>\n\n          <tr>\n            <td style=\"font-size: 12px\">\n              Refresh ပြုလုပ်ရန်ကျန်ရှိသောအရေအတွက်\n            </td>\n            <td style=\"font-size: 12px\">\n              {{ (PostTotal = +(no_of_refresh_ad - today_no_of_refresh)) }}\n            </td>\n          </tr>\n        </tbody>\n      </template>\n    </v-simple-table>\n\n    <div v-if=\"no_of_refresh_ad == 0\">\n      <PackageNoOrderFefresh />\n    </div>\n\n    <div v-else-if=\"PostTotal == 0\">\n      <DailyRefreshAlert />\n    </div>\n\n    <div v-else>\n      <v-alert color=\"warning\">\n        <div class=\"\" style=\"color: white; font-weight: bold; font-size: 20px\">\n          ဤအိမ်ခြံမြေအား Refresh AD ပြုလုပ်မည်\n        </div>\n        <br />\n        <p style=\"color: white; font-weight: bold\">\n          {{ propertyDetail.title_mm }}\n          <br />\n          ကြော်ငြာနံပါတ် - {{ propertyDetail.ad_number }}\n          <br />\n          စျေးနှုန်း -\n          {{ propertyDetail.price }}\n          {{\n            propertyDetail.price_type == \"MMK(Lakhs)\" ? \"သိန်း(ကျပ်)\" : \"USD\"\n          }}\n        </p>\n      </v-alert>\n\n      <v-btn block color=\"warning\" @click=\"RefreshAdNow($route.params.id)\">\n        Refresh AD\n      </v-btn>\n    </div>\n  </div>\n</template>\n\n<script>\nimport { HTTP } from \"@/use/http-common\";\nimport Loader from \"@/components/Loader\";\nimport PackageNoOrderFefresh from \"@/components/PackageNoOrderFefresh\";\nimport DailyRefreshAlert from \"@/components/DailyRefreshAlert\";\n\nexport default {\n  name: \"RefreshProperty\",\n\n  components: {\n    Loader,\n    PackageNoOrderFefresh,\n    DailyRefreshAlert,\n  },\n\n  data() {\n    return {\n      isLoad: true,\n      tnwuserdatastore: {},\n      no_of_refresh_ad: 0,\n      today_no_of_refresh: 0,\n      success: \"\",\n      error: \"\",\n\n      propertyDetail: {},\n      propertyPhotos: [],\n      tatnaywon: {},\n    };\n  },\n\n  methods: {\n    swipe(direction) {\n      this.swipeDirection = direction;\n      if (direction) {\n        if (direction === \"Right\") {\n          this.$router.go(-1);\n        }\n      }\n    },\n\n    async fetch_package() {\n      var userId = this.tnwuserdatastore.user_id;\n      HTTP.get(`package/number_of_refresh_daily/${userId}`)\n        .then((response) => {\n          this.no_of_refresh_ad = response.data.number_of_refresh_daily;\n        })\n        .catch((e) => {\n          this.no_of_refresh_ad = 0;\n        });\n    },\n\n    async today_refresh_ad_counter() {\n      var userId = this.tnwuserdatastore.user_id;\n      HTTP.get(`refres_ad/index/${userId}`)\n        .then((response) => {\n          this.today_no_of_refresh = response.data.today_refresh_ad_counter;\n        })\n        .catch((e) => {\n          this.today_no_of_refresh = 0;\n        });\n    },\n\n    RefreshAdNow: function (id) {\n      var userId = this.tnwuserdatastore.user_id;\n      HTTP.get(`property/refreshad/${id}/${userId}`)\n        .then((response) => {\n          if (response.status === 200) {\n            this.success = \"Refresh AD ပြီးပါပြီ\";\n            this.error = \"\";\n\n            this.fetch_package();\n            this.today_refresh_ad_counter();\n          }\n        })\n        .catch((e) => {\n          this.error = \"မအောင်မြင်ပါ\";\n          this.success = \"\";\n        });\n    },\n  },\n\n  mounted() {\n    this.fetch_package();\n    this.today_refresh_ad_counter();\n  },\n\n  created() {\n    let tnwuserstore = localStorage.getItem(\"tnwuserstore\");\n    this.tnwuserdatastore = JSON.parse(tnwuserstore);\n\n    HTTP.get(`property/detail/${this.$route.params.id}`)\n      .then((response) => {\n        this.propertyDetail = response.data.data;\n        this.propertyPhotos.push(...response.data.photo);\n        this.tatnaywon = response.data.tatnaywon;\n        this.isLoad = false;\n      })\n      .catch((e) => {\n        this.isLoad = true;\n      });\n  },\n};\n</script>"],"sourceRoot":"src/views/refresh"}]}