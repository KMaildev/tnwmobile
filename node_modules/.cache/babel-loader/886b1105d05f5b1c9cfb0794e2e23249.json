AAAA;AAAA;AAAA;AAAA;AAAA;AACA,gBAAA,IAAA,CAAA,GAAA,uBAAA,KAAA,CAAA,MAAA,CAAA,MAAA,CAAA,WAAA,cAAA,KAAA,CAAA,IAAA,GACA,IADA,CACA,UAAA,QAAA,EAAA;AAAA;;AACA,kCAAA,KAAA,CAAA,MAAA,EAAA,IAAA,wCAAA,QAAA,CAAA,IAAA,CAAA,IAAA;;AACA,kBAAA,KAAA,CAAA,MAAA,GAAA,KAAA;AACA,iBAJA,WAKA,UAAA,CAAA,EAAA;AACA,kBAAA,KAAA,CAAA,MAAA,GAAA,IAAA;AACA,iBAPA;;AADA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AASA,KAVA;AAYA,IAAA,oBAZA,gCAYA,SAZA,EAYA;AACA,UAAA,CAAA,SAAA,EAAA;AACA;AACA;;AACA,WAAA,IAAA;AACA,WAAA,KAAA;AACA,KAlBA;AAoBA,IAAA,KApBA,iBAoBA,EApBA,EAoBA;AACA,MAAA,SAAA,CACA,KADA,CACA;AACA,QAAA,KAAA,EAAA,uBADA;AAEA,QAAA,GAAA,EAAA,YAAA,GAAA,gBAAA,GAAA;AAFA,OADA,EAKA,IALA,CAKA,UAAA,CAAA;AAAA,eAAA,OAAA,CAAA,GAAA,CAAA,uBAAA,CAAA;AAAA,OALA,WAMA,UAAA,KAAA;AAAA,eACA,OAAA,CAAA,GAAA,CAAA,sCAAA,EAAA,KAAA,CADA;AAAA,OANA;AASA;AA9BA,GAlBA;AAmDA,EAAA,KAAA,EAAA;AACA,IAAA,MADA,kBACA,EADA,EACA,IADA,EACA;AACA,UAAA,SAAA,GAAA,IAAA,CAAA,MAAA,CAAA,WAAA;AACA,UAAA,YAAA,GAAA,KAAA,MAAA,CAAA,MAAA,CAAA,WAAA;;AACA,UAAA,SAAA,IAAA,YAAA,EAAA;AACA,aAAA,IAAA,GAAA,CAAA;AACA,aAAA,KAAA;AACA;AACA;AARA,GAnDA;AA8DA,EAAA,OA9DA,qBA8DA;AACA,SAAA,IAAA,GAAA,CAAA;AACA,SAAA,KAAA;AACA;AAjEA,CAAA","sourcesContent":["<template>\n  <div>\n    <Loader v-show=\"isLoad\" />\n    <v-card\n      v-show=\"!isLoad\"\n      style=\"background-color: white; margin: 3px\"\n      class=\"mx-auto\"\n      max-width=\"auto\"\n      outlined\n      v-for=\"(agent, index) in agents\"\n      :key=\"index\"\n    >\n      <v-list-item three-line>\n        <v-list-item-content>\n          <v-list-item-title\n            class=\"mb-1\"\n            v-html=\"agent.company_name == '' ? agent.name : agent.company_name\"\n          >\n          </v-list-item-title>\n          <v-list-item-subtitle v-html=\"agent.address\"></v-list-item-subtitle>\n        </v-list-item-content>\n\n        <v-list-item-avatar tile size=\"80\" color=\"#f9f7f7\">\n          <img v-if=\"agent.logo\" :src=\"IMAGE_URL + agent.logo\" alt=\"Logo\" />\n          <v-avatar\n            v-else\n            color=\"grey lighten-1\"\n            size=\"70\"\n            tile\n            style=\"padding: 5px; margin: 10px\"\n          >\n            <span class=\"white--text text-h5\">\n              {{\n                agent.company_name == \"\"\n                  ? agent.name.substring(0, 1)\n                  : agent.company_name.substring(0, 1)\n              }}\n            </span>\n          </v-avatar>\n        </v-list-item-avatar>\n      </v-list-item>\n\n      <v-card-actions style=\"background-color: #f7f7f7; padding: 1px\">\n        <v-spacer></v-spacer>\n        <v-dialog transition=\"dialog-bottom-transition\">\n          <template v-slot:activator=\"{ on, attrs }\">\n            <v-btn\n              color=\"callBtn\"\n              v-bind=\"attrs\"\n              v-on=\"on\"\n              x-small\n              class=\"white--text\"\n            >\n              <v-icon left small>mdi-phone</v-icon>\n              ဆက်သွယ်ရန်\n            </v-btn>\n          </template>\n\n          <template>\n            <v-card>\n              <v-toolbar color=\"warning\" dark small>\n                <span style=\"font-size: 11px\">\n                  {{\n                    agent.company_name == \"\" ? agent.name : agent.company_name\n                  }}\n                  အတွက် ဆက်သွယ်ရန် ဖုန်းနံပါတ်</span\n                >\n              </v-toolbar>\n              <v-card-text>\n                <div class=\"pa-1\">\n                  <a\n                    v-for=\"ph in agent.phone.split(',')\"\n                    :href=\"`tel:${ph}`\"\n                    style=\"color: black; text-decoration: none\"\n                  >\n                    {{ ph }} <br />\n                  </a>\n                </div>\n              </v-card-text>\n            </v-card>\n          </template>\n        </v-dialog>\n\n        <v-col cols=\"auto\">\n          <v-btn\n            color=\"shareBtn\"\n            x-small\n            class=\"white--text\"\n            @click=\"share(agent.user_id)\"\n          >\n            <v-icon left small> mdi-share-variant </v-icon>\n            မျှဝေရန်\n          </v-btn>\n        </v-col>\n\n        <v-btn\n          :to=\"{ name: 'agentDetail', params: { id: agent.user_id } }\"\n          color=\"detailBtn\"\n          x-small\n          class=\"white--text\"\n        >\n          <v-icon left small>mdi-eye</v-icon>\n          ကြည့်ရန်\n        </v-btn>\n        <v-spacer></v-spacer>\n      </v-card-actions>\n    </v-card>\n\n    <div v-if=\"agents.length\" v-observe-visibility=\"handleScrollToButton\"></div>\n  </div>\n</template>\n\n<script>\nimport { HTTP } from \"@/use/http-common\";\nimport { IMAGE_URL, LogoNotFound, APP_BASE_URL } from \"@/use/image-url\";\nimport Loader from \"@/components/Loader\";\n\nexport default {\n  name: \"Agent\",\n\n  components: {\n    Loader,\n  },\n\n  data() {\n    return {\n      isLoad: true,\n      agents: [],\n      page: 0,\n      IMAGE_URL: IMAGE_URL,\n      LogoNotFound: LogoNotFound,\n      APP_BASE_URL: APP_BASE_URL,\n    };\n  },\n\n  methods: {\n    async fetch() {\n      HTTP.get(`agent/index/${this.$route.params.companytype}/${this.page}`)\n        .then((response) => {\n          this.agents.push(...response.data.data);\n          this.isLoad = false;\n        })\n        .catch((e) => {\n          this.isLoad = true;\n        });\n    },\n\n    handleScrollToButton(isVisible) {\n      if (!isVisible) {\n        return;\n      }\n      this.page++;\n      this.fetch();\n    },\n\n    share(id) {\n      navigator\n        .share({\n          title: \"tatnaywonproperty.com\",\n          url: APP_BASE_URL + \"agents/detail/\" + id,\n        })\n        .then((_) => console.log(\"Yay, you shared it :)\"))\n        .catch((error) =>\n          console.log(\"Oh noh! You couldn't share it! :'(\\n\", error)\n        );\n    },\n  },\n\n  watch: {\n    $route(to, from) {\n      const fromParam = from.params.companytype;\n      const currentParam = this.$route.params.companytype;\n      if (fromParam != currentParam) {\n        this.page = 0;\n        this.fetch();\n      }\n    },\n  },\n\n  mounted() {\n    this.page = 0;\n    this.fetch();\n  },\n};\n</script>\n"],"sourceRoot":"src/views/agent"}]}