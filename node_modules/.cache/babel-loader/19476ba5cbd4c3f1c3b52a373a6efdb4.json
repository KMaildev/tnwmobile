A;AAmBA,GAxBA;AA0BA,EAAA,OAAA,EAAA;AACA,IAAA,cADA,4BACA;AAAA;;AAAA;AAAA;AAAA;AAAA;AAAA;AACA,gBAAA,IAAA,CAAA,GAAA,sCACA,KAAA,CAAA,IADA,oBACA,KAAA,CAAA,KADA,oBACA,KAAA,CAAA,KADA,0BACA,KAAA,CAAA,WADA,uBACA,KAAA,CAAA,QADA,qBACA,KAAA,CAAA,MADA,GAGA,IAHA,CAGA,UAAA,QAAA,EAAA;AACA,sBAAA,QAAA,CAAA,MAAA,KAAA,GAAA,EAAA;AACA,oBAAA,KAAA,CAAA,OAAA,GAAA,+BAAA;AACA;AACA,iBAPA,WAQA,UAAA,CAAA,EAAA;AACA,kBAAA,KAAA,CAAA,QAAA,GAAA,+BAAA;AACA,iBAVA;;AADA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAYA,KAbA;AAeA,IAAA,gBAfA,8BAeA;AAAA;;AAAA;AAAA;AAAA;AAAA;AAAA;AACA,gBAAA,IAAA,CAAA,GAAA,iDACA,MAAA,CAAA,IADA,mBACA,MAAA,CAAA,IADA,oBACA,MAAA,CAAA,YADA,qBACA,MAAA,CAAA,MADA,GAGA,IAHA,CAGA,UAAA,QAAA,EAAA;AACA,sBAAA,QAAA,CAAA,MAAA,KAAA,GAAA,EAAA;AACA,oBAAA,MAAA,CAAA,OAAA,GAAA,+BAAA;AACA;AACA,iBAPA,WAQA,UAAA,CAAA,EAAA;AACA,kBAAA,MAAA,CAAA,QAAA,GAAA,+BAAA;AACA,iBAVA;;AADA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAYA;AA3BA;AA1BA,CAAA","sourcesContent":["<template>\n  <div>\n    <v-alert dense outlined type=\"success\" v-if=\"success\" style=\"margin: 2px\">\n      {{ success }}\n    </v-alert>\n\n    <v-alert dense outlined type=\"error\" v-if=\"messages\" style=\"margin: 2px\">\n      {{ messages }}\n    </v-alert>\n\n    <form @submit.prevent=\"RequestInquiry\">\n      <v-card-text>\n        <v-text-field\n          v-model=\"name\"\n          :rules=\"[() => !!name || 'This field is required']\"\n          label=\"သင်၏အမည် (Name)\"\n          required\n        ></v-text-field>\n\n        <v-text-field\n          ref=\"phone\"\n          v-model=\"phone\"\n          :rules=\"[() => !!phone || 'This field is required']\"\n          label=\"ဆက်သွယ်ရန်ဖုန်း (Phone)\"\n          required\n        ></v-text-field>\n\n        <v-text-field\n          ref=\"email\"\n          v-model=\"email\"\n          label=\"အီးမေးလ်း (Email / Gmail)\"\n        ></v-text-field>\n\n        <v-text-field\n          ref=\"subjects\"\n          v-model=\"subjects\"\n          label=\"အကြောင်းအရာ\"\n        ></v-text-field>\n\n        <v-textarea\n          v-model=\"description\"\n          label=\"သင်မေးလိုသော အကြောင်းအရာအသေးစိတ်\"\n          :rules=\"[() => !!description || 'This field is required']\"\n          required\n          rows=\"3\"\n        ></v-textarea>\n\n        <v-btn class=\"ma-1\" color=\"warning\" dark type=\"submit\" block>\n          ဆက်သွယ်ပါ\n        </v-btn>\n      </v-card-text>\n    </form>\n\n    <form @submit.prevent=\"TakeAppointments\" id=\"formbottom\">\n      <v-card-text>\n        <v-row>\n          <v-col cols=\"6\" sm=\"6\" md=\"6\">\n            <v-dialog\n              ref=\"dialog\"\n              v-model=\"modal\"\n              :return-value.sync=\"date\"\n              persistent\n              width=\"290px\"\n            >\n              <template v-slot:activator=\"{ on, attrs }\">\n                <v-text-field\n                  v-model=\"date\"\n                  label=\"Appointment Date\"\n                  prepend-icon=\"mdi-calendar\"\n                  readonly\n                  v-bind=\"attrs\"\n                  v-on=\"on\"\n                ></v-text-field>\n              </template>\n              <v-date-picker v-model=\"date\" scrollable>\n                <v-spacer></v-spacer>\n                <v-btn text color=\"primary\" @click=\"modal = false\">\n                  Cancel\n                </v-btn>\n                <v-btn text color=\"primary\" @click=\"$refs.dialog.save(date)\">\n                  OK\n                </v-btn>\n              </v-date-picker>\n            </v-dialog>\n          </v-col>\n\n          <v-col cols=\"6\" sm=\"6\" md=\"6\">\n            <v-dialog\n              ref=\"dialogtime\"\n              v-model=\"modal2\"\n              :return-value.sync=\"time\"\n              persistent\n              width=\"290px\"\n            >\n              <template v-slot:activator=\"{ on, attrs }\">\n                <v-text-field\n                  v-model=\"time\"\n                  label=\"Appointment Time\"\n                  prepend-icon=\"mdi-clock-time-four-outline\"\n                  readonly\n                  v-bind=\"attrs\"\n                  v-on=\"on\"\n                ></v-text-field>\n              </template>\n              <v-time-picker v-if=\"modal2\" v-model=\"time\" full-width>\n                <v-spacer></v-spacer>\n                <v-btn text color=\"primary\" @click=\"modal2 = false\">\n                  Cancel\n                </v-btn>\n                <v-btn\n                  text\n                  color=\"primary\"\n                  @click=\"$refs.dialogtime.save(time)\"\n                >\n                  OK\n                </v-btn>\n              </v-time-picker>\n            </v-dialog>\n          </v-col>\n        </v-row>\n\n        <v-text-field\n          label=\"ဆက်သွယ်ရန်ဖုန်း (Phone)\"\n          ref=\"phone\"\n          v-model=\"phone_number\"\n          :rules=\"[() => !!phone_number || 'This field is required']\"\n          prepend-icon=\"mdi-file-phone\"\n        ></v-text-field>\n\n        <v-btn class=\"ma-1\" color=\"warning\" dark type=\"submit\" block>\n          ရက်ချိန်းရယူပါ။\n        </v-btn>\n      </v-card-text>\n    </form>\n  </div>\n</template>\n<script>\nimport { HTTP } from \"@/use/http-common\";\nexport default {\n  name: \"AgentRequestInquiry\",\n  props: [\"userId\"],\n\n  data() {\n    return {\n      messages: \"\",\n      success: \"\",\n      date: new Date(Date.now() - new Date().getTimezoneOffset() * 60000)\n        .toISOString()\n        .substr(0, 10),\n      menu: false,\n      modal: false,\n\n      time: null,\n      modal2: false,\n\n      name: \"\",\n      phone: \"\",\n      email: \"\",\n      subjects: \"ကြော်ငြာနံပါတ် S-000153 အကြောင်းကို ပိုမိုသိလိုသည်။\",\n      description: \"\",\n      phone_number: \"\",\n    };\n  },\n\n  methods: {\n    async RequestInquiry() {\n      HTTP.get(\n        `request_inquiry/index?name=${this.name}&phone=${this.phone}&email=${this.email}&description=${this.description}&subjects=${this.subjects}&userId=${this.userId}`\n      )\n        .then((response) => {\n          if (response.status === 200) {\n            this.success = \"လုပ်ဆောင်ချက် အောင်မြင်ပါသည်။\";\n          }\n        })\n        .catch((e) => {\n          this.messages = \"အချက်အလက်များကို ထည့်သွင်းပါ။\";\n        });\n    },\n\n    async TakeAppointments() {\n      HTTP.get(\n        `request_inquiry/takeappointments?date=${this.date}&time=${this.time}&phone=${this.phone_number}&userId=${this.userId}`\n      )\n        .then((response) => {\n          if (response.status === 200) {\n            this.success = \"လုပ်ဆောင်ချက် အောင်မြင်ပါသည်။\";\n          }\n        })\n        .catch((e) => {\n          this.messages = \"အချက်အလက်များကို ထည့်သွင်းပါ။\";\n        });\n    },\n  },\n};\n</script>"],"sourceRoot":"src/components"}]}