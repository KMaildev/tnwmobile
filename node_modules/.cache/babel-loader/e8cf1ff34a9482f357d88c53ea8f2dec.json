A,CAAA,WAAA,CAAA,KAHA;AAKA,gBAAA,iBALA,GAKA,KAAA,CAAA,eAAA,CAAA,KAAA,CAAA,WAAA,CALA;AAMA,gBAAA,cANA,GAMA,KAAA,CAAA,qBAAA,CAAA,KAAA,CAAA,WAAA,CANA;AAQA,gBAAA,eARA,GAQA,KAAA,CAAA,WAAA,CAAA,KAAA,CAAA,WAAA,CARA;AASA,gBAAA,YATA,GASA,KAAA,CAAA,eAAA,CAAA,KAAA,CAAA,WAAA,CATA;AAWA,gBAAA,IAAA,CAAA,GAAA,0CACA,KAAA,CAAA,OADA,oCACA,KAAA,CAAA,qBADA,2BACA,KAAA,CAAA,MADA,8BACA,eADA,gCACA,iBADA,6BACA,cADA,8BACA,eADA,2BACA,YADA,sBACA,MADA,yBACA,KAAA,CAAA,MAAA,CAAA,MAAA,CAAA,EADA,GAGA,IAHA,CAGA,UAAA,QAAA,EAAA;AACA,sBAAA,QAAA,CAAA,MAAA,KAAA,GAAA,EAAA;AACA,oBAAA,KAAA,CAAA,OAAA,GAAA,8BAAA;AACA;AACA,iBAPA,WAQA,UAAA,CAAA,EAAA;AACA,kBAAA,KAAA,CAAA,YAAA,GAAA,+BAAA;AACA,iBAVA;;AAXA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAsBA,KAvBA;AAyBA,IAAA,iBAzBA,+BAyBA;AAAA;;AAAA;AAAA;AAAA;AAAA;AAAA;AACA,gBAAA,IAAA,CAAA,GAAA,iCAAA,MAAA,CAAA,MAAA,CAAA,MAAA,CAAA,EAAA,GACA,IADA,CACA,UAAA,QAAA,EAAA;AACA,kBAAA,MAAA,CAAA,WAAA,GAAA,QAAA,CAAA,IAAA,CAAA,IAAA;AACA,iBAHA,WAIA,UAAA,CAAA,EAAA;AACA,kBAAA,OAAA,CAAA,GAAA,CAAA,CAAA;AACA,iBANA;;AADA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAQA,KAjCA;AAmCA,IAAA,eAnCA,2BAmCA,OAnCA,EAmCA;AACA,aAAA,OAAA,CAAA,KAAA,GAAA,OAAA,CAAA,KAAA,GAAA,OAAA,CAAA,QAAA,GAAA,GAAA;AACA,KArCA;AAuCA,IAAA,qBAvCA,iCAuCA,OAvCA,EAuCA;AACA,UAAA,eAAA,GACA,CAAA,OAAA,CAAA,KAAA,GAAA,OAAA,CAAA,KAAA,GAAA,OAAA,CAAA,QAAA,GAAA,GADA;AAEA,UAAA,cAAA,GACA,CAAA,eAAA,GAAA,eAAA,GAAA,OAAA,CAAA,QAAA,GAAA,GADA;AAEA,aAAA,cAAA;AACA,KA7CA;AA+CA,IAAA,WA/CA,uBA+CA,OA/CA,EA+CA;AACA,aAAA,OAAA,CAAA,KAAA,GAAA,OAAA,CAAA,KAAA,GAAA,OAAA,CAAA,kBAAA,GAAA,GAAA;AACA,KAjDA;AAmDA,IAAA,eAnDA,2BAmDA,OAnDA,EAmDA;AACA,UAAA,aAAA,GACA,CAAA,OAAA,CAAA,KAAA,GAAA,OAAA,CAAA,KAAA,GAAA,OAAA,CAAA,kBAAA,GAAA,GADA;AAEA,UAAA,cAAA,GACA,CAAA,aAAA,GAAA,aAAA,GAAA,OAAA,CAAA,kBAAA,GAAA,GADA;AAEA,aAAA,cAAA;AACA;AAzDA,GA3BA;AAuFA,EAAA,OAvFA,qBAuFA;AACA,SAAA,iBAAA;AACA,GAzFA;AA2FA,EAAA,OA3FA,qBA2FA;AACA,QAAA,YAAA,GAAA,YAAA,CAAA,OAAA,CAAA,cAAA,CAAA;AACA,SAAA,gBAAA,GAAA,IAAA,CAAA,KAAA,CAAA,YAAA,CAAA;AACA;AA9FA,CAAA","sourcesContent":["<template>\n  <div>\n    <v-list class=\"transparent\">\n      <div style=\"padding: 5px; background-color: #af742e\" class=\"card-panel\">\n        <center style=\"color: white\">\n          {{ packageplan.package_name }}\n        </center>\n      </div>\n      <v-simple-table style=\"background-color: #f2efef\">\n        <template v-slot:default>\n          <tbody>\n            <tr>\n              <td style=\"font-size: 12px\">One Month</td>\n              <td style=\"font-size: 12px\">\n                {{ packageplan.price }}\n              </td>\n            </tr>\n\n            <tr>\n              <td style=\"font-size: 12px\">\n                5% Discount For 3 Months Contract\n                <span style=\"font-size: 12px\"> (Monthly Payment)</span>\n              </td>\n              <td style=\"font-size: 12px\">\n                {{ discountedPrice(packageplan) }}\n              </td>\n            </tr>\n\n            <tr>\n              <td style=\"font-size: 12px\">Additional (Full Payment)</td>\n              <td style=\"font-size: 12px\">\n                {{ DoubleDiscountedPrice(packageplan) }}\n              </td>\n            </tr>\n\n            <tr>\n              <td style=\"font-size: 12px\">\n                10% Discount For 6 Months Contract\n                <span style=\"font-size: 12px\"> (Monthly Payment)</span>\n              </td>\n              <td style=\"font-size: 12px\">\n                {{ sixMonthDis(packageplan) }}\n              </td>\n            </tr>\n\n            <tr>\n              <td style=\"font-size: 12px\">Additional (Full Payment)</td>\n              <td style=\"font-size: 12px\">\n                {{ sixMonthDisFull(packageplan) }}\n              </td>\n            </tr>\n          </tbody>\n        </template>\n      </v-simple-table>\n\n      <v-alert\n        dense\n        outlined\n        type=\"error\"\n        v-if=\"alertMessage\"\n        style=\"margin: 2px\"\n      >\n        {{ alertMessage }}\n      </v-alert>\n\n      <v-alert dense outlined type=\"success\" v-if=\"success\" style=\"margin: 2px\">\n        {{ success }}\n      </v-alert>\n\n      <form @submit.prevent=\"OrderNow\">\n        <v-card flat>\n          <v-card-text>\n            <v-text-field\n              label=\"Phone number or email address\"\n              v-model=\"contact\"\n            ></v-text-field>\n\n            <v-select\n              :items=\"choosepayments\"\n              item-text=\"title\"\n              item-value=\"value\"\n              label=\"Choose Payment\"\n              v-model=\"choose_payment_status\"\n            ></v-select>\n\n            <v-radio-group v-model=\"radios\">\n              <template v-slot:label>\n                <div><strong>Billing Cycle</strong></div>\n              </template>\n              <v-radio value=\"1\">\n                <template v-slot:label>\n                  <div>\n                    1 Month @\n                    <strong class=\"primary--text\">\n                      {{ packageplan.price }}\n                    </strong>\n                  </div>\n                </template>\n              </v-radio>\n              <v-radio value=\"3MonthsContractMonthlyPayment\">\n                <template v-slot:label>\n                  <div>\n                    5% Discount For 3 Months Contract (Monthly Payment) @\n                    <strong class=\"primary--text\">\n                      {{ discountedPrice(packageplan) }}\n                    </strong>\n                  </div>\n                </template>\n              </v-radio>\n\n              <v-radio value=\"3MonthsFullPayment\">\n                <template v-slot:label>\n                  <div>\n                    3 Months Additional (Full Payment)\n                    <strong class=\"primary--text\">\n                      {{ DoubleDiscountedPrice(packageplan) }}\n                    </strong>\n                  </div>\n                </template>\n              </v-radio>\n\n              <v-radio value=\"6MonthsContractMonthlyPayment\">\n                <template v-slot:label>\n                  <div>\n                    10% Discount For 6 Months Contract (Monthly Payment) @\n                    <strong class=\"primary--text\">\n                      {{ sixMonthDis(packageplan) }}\n                    </strong>\n                  </div>\n                </template>\n              </v-radio>\n\n              <v-radio value=\"6MonthsFullPayment\">\n                <template v-slot:label>\n                  <div>\n                    6 Months Additional (Full Payment)\n                    <strong class=\"primary--text\">\n                      {{ sixMonthDisFull(packageplan) }}\n                    </strong>\n                  </div>\n                </template>\n              </v-radio>\n            </v-radio-group>\n\n            <v-btn type=\"submit\" block color=\"warning\" dark> Order Now </v-btn>\n          </v-card-text>\n        </v-card>\n      </form>\n    </v-list>\n  </div>\n</template>\n<script>\nimport { HTTP } from \"@/use/http-common\";\nexport default {\n  name: \"OrderNow\",\n  data() {\n    return {\n      packageplan: {},\n      tnwuserdatastore: {},\n      success: \"\",\n      alertMessage: \"\",\n\n      choosepayments: [\n        {\n          title: \"Contact me\",\n          value: \"Contact me\",\n        },\n\n        {\n          title: \"Bank Transfer\",\n          value: \"Bank Transfer\",\n        },\n      ],\n\n      radios: \"1\",\n      contact: \"\",\n      choose_payment_status: \"\",\n    };\n  },\n\n  methods: {\n    async OrderNow() {\n      let userId = this.tnwuserdatastore.user_id;\n\n      let OneMonthlyPrice = this.packageplan.price;\n\n      let ThreeMonthlyPrice = this.discountedPrice(this.packageplan);\n      let ThreeFullPrice = this.DoubleDiscountedPrice(this.packageplan);\n\n      let SixMonthlyPrice = this.sixMonthDis(this.packageplan);\n      let SixFullPrice = this.sixMonthDisFull(this.packageplan);\n\n      HTTP.get(\n        `package/ordernow?contactstatus=${this.contact}&choose_payment_status=${this.choose_payment_status}&BillingCycle=${this.radios}&OneMonthlyPrice=${OneMonthlyPrice}&ThreeMonthlyPrice=${ThreeMonthlyPrice}&ThreeFullPrice=${ThreeFullPrice}&SixMonthlyPrice=${SixMonthlyPrice}&SixFullPrice=${SixFullPrice}&user_id=${userId}&package_id=${this.$route.params.id}`\n      )\n        .then((response) => {\n          if (response.status === 200) {\n            this.success = \"လုပ်ဆောင်ချက် အောင်မြင်ပါသည်\";\n          }\n        })\n        .catch((e) => {\n          this.alertMessage = \"အချက်အလက်များကို ထည့်သွင်းပါ။\";\n        });\n    },\n\n    async fetch_packageplan() {\n      HTTP.get(`package/packagedetail/${this.$route.params.id}`)\n        .then((response) => {\n          this.packageplan = response.data.data;\n        })\n        .catch((e) => {\n          console.log(e);\n        });\n    },\n\n    discountedPrice(product) {\n      return product.price - (product.price * product.discount) / 100;\n    },\n\n    DoubleDiscountedPrice(product) {\n      var threeMonthValue =\n        +product.price - (product.price * product.discount) / 100;\n      var threeMonthFull =\n        +threeMonthValue - (threeMonthValue * product.discount) / 100;\n      return threeMonthFull;\n    },\n\n    sixMonthDis(product) {\n      return product.price - (product.price * product.six_month_discount) / 100;\n    },\n\n    sixMonthDisFull(product) {\n      var sixMonthValue =\n        +product.price - (product.price * product.six_month_discount) / 100;\n      var threeMonthFull =\n        +sixMonthValue - (sixMonthValue * product.six_month_discount) / 100;\n      return threeMonthFull;\n    },\n  },\n\n  mounted() {\n    this.fetch_packageplan();\n  },\n\n  created() {\n    let tnwuserstore = localStorage.getItem(\"tnwuserstore\");\n    this.tnwuserdatastore = JSON.parse(tnwuserstore);\n  },\n};\n</script>\n"],"sourceRoot":"src/views/Package"}]}