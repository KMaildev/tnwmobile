e=\"opacity: 0.22\"></v-divider>\n        <v-row align=\"center\" no-gutters>\n          <v-col class=\"grow\">\n            <p\n              style=\"font-size: 12px; color: black\"\n              v-html=\"guideDetail.description\"\n            ></p>\n            <div></div>\n          </v-col>\n        </v-row>\n      </v-alert>\n    </v-card>\n  </div>\n</template>\n\n<script>\nimport { HTTP } from \"@/use/http-common\";\nimport Loader from \"@/components/Loader\";\nimport { IMAGE_URL } from \"@/use/image-url\";\n\nexport default {\n  name: \"BookDetail\",\n\n  components: {\n    Loader,\n  },\n\n  data() {\n    return {\n      isLoad: true,\n      IMAGE_URL: IMAGE_URL,\n      guideDetail: {},\n    };\n  },\n\n  methods: {\n    swipe(direction) {\n      this.swipeDirection = direction;\n      if (direction) {\n        if (direction === \"Right\") {\n          this.$router.go(-1);\n        }\n      }\n    },\n  },\n\n  created() {\n    HTTP.get(`books/detail/${this.$route.params.id}`)\n      .then((response) => {\n        this.guideDetail = response.data.data;\n        this.isLoad = false;\n      })\n      .catch((e) => {\n        this.isLoad = true;\n      });\n  },\n};\n</script>"]}]}