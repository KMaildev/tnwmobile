     <v-carousel-item\n          v-for=\"(propertyPhoto, index) in propertyPhotos\"\n          :key=\"index\"\n        >\n          <img\n            :src=\"propertyPhoto.image_url + propertyPhoto.photo\"\n            alt=\"Property Image\"\n            style=\"width: 100%\"\n          />\n        </v-carousel-item>\n      </v-carousel>\n    </v-card>\n  </div>\n</template>\n\n<script>\nimport { HTTP } from \"@/use/http-common\";\nimport Loader from \"@/components/Loader\";\nimport { APP_BASE_URL } from \"@/use/image-url\";\n\nexport default {\n  name: \"PropertyImage\",\n\n  components: {\n    Loader,\n  },\n\n  data() {\n    return {\n      isLoad: true,\n      propertyPhotos: [],\n      APP_BASE_URL: APP_BASE_URL,\n\n      tnwuserdatastore: {},\n      loggedIn: false,\n    };\n  },\n\n  methods: {\n    swipe(direction) {\n      this.swipeDirection = direction;\n      if (direction) {\n        if (direction === \"Right\") {\n          this.$router.go(-1);\n        }\n      }\n    },\n  },\n\n  created() {\n    HTTP.get(`property/detail/${this.$route.params.id}`)\n      .then((response) => {\n        this.propertyPhotos.push(...response.data.photo);\n        this.tatnaywon = response.data.tatnaywon;\n        this.isLoad = false;\n      })\n      .catch((e) => {\n        this.isLoad = true;\n      });\n  },\n};\n</script>\n\n<style>\n#create .v-speed-dial {\n  position: absolute;\n}\n\n#create .v-btn--floating {\n  position: relative;\n}\n</style>\n"]}]}