form @submit.prevent=\"update_property\" id=\"formbottom\">\n            <v-card flat>\n              <v-card-text>\n                <div>\n                  <v-text-field\n                    label=\"Title\"\n                    v-model=\"propertyDetail.title_mm\"\n                    autocomplete=\"on\"\n                    required\n                  ></v-text-field>\n\n                  <v-text-field\n                    label=\"Title Eng (optional)\"\n                    v-model=\"propertyDetail.title_eng\"\n                    autocomplete=\"on\"\n                  ></v-text-field>\n\n                  <v-select\n                    :items=\"propertytype\"\n                    item-text=\"property_type_mm\"\n                    item-value=\"property_type_id\"\n                    label=\"Property Type\"\n                    v-model=\"propertyDetail.property_type_id\"\n                    required\n                  ></v-select>\n\n                  <v-row>\n                    <v-col cols=\"6\" sm=\"6\">\n                      <v-text-field\n                        type=\"number\"\n                        label=\"Price\"\n                        v-model=\"propertyDetail.price\"\n                        autocomplete=\"on\"\n                        required\n                      ></v-text-field>\n                    </v-col>\n\n                    <v-col cols=\"6\" sm=\"6\">\n                      <v-select\n                        :items=\"currency\"\n                        item-text=\"name\"\n                        item-value=\"value\"\n                        v-model=\"propertyDetail.price_type\"\n                      ></v-select>\n                    </v-col>\n                  </v-row>\n\n                  <v-select\n                    :items=\"propertystatus\"\n                    item-text=\"title\"\n                    item-value=\"value\"\n                    label=\"Status / တိုက်ခန်း အခြေအနေ\"\n                    v-model=\"propertyDetail.property_status\"\n                    required\n                  ></v-select>\n\n                  <v-row>\n                    <v-col cols=\"6\" sm=\"6\">\n                      <v-text-field\n                        label=\"Area\"\n                        v-model=\"propertyDetail.area\"\n                        autocomplete=\"on\"\n                      ></v-text-field>\n                    </v-col>\n\n                    <v-col cols=\"6\" sm=\"6\">\n                      <v-select\n                        :items=\"areatypes\"\n                        item-text=\"title\"\n                        item-value=\"value\"\n                        v-model=\"propertyDetail.area_type\"\n                        required\n                      ></v-select>\n                    </v-col>\n                  </v-row>\n\n                  <v-select\n                    :items=\"rooms\"\n                    item-text=\"title\"\n                    item-value=\"value\"\n                    label=\"Room\"\n                    v-model=\"propertyDetail.rooms\"\n                  ></v-select>\n\n                  <v-select\n                    :items=\"rooms\"\n                    item-text=\"title\"\n                    item-value=\"value\"\n                    label=\"Bathroom\"\n                    v-model=\"propertyDetail.bathroom\"\n                  ></v-select>\n\n                  <v-select\n                    :items=\"rooms\"\n                    item-text=\"title\"\n                    item-value=\"value\"\n                    label=\"Bedroom\"\n                    v-model=\"propertyDetail.bead_room\"\n                  ></v-select>\n\n                  <v-select\n                    :items=\"rooms\"\n                    item-text=\"title\"\n                    item-value=\"value\"\n                    label=\"Master Bedroom\"\n                    v-model=\"propertyDetail.masterbathroom\"\n                  ></v-select>\n\n                  <v-select\n                    :items=\"rooms\"\n                    item-text=\"title\"\n                    item-value=\"value\"\n                    label=\"Living Room\"\n                    v-model=\"propertyDetail.living_room\"\n                  ></v-select>\n\n                  <v-select\n                    :items=\"floors\"\n                    item-text=\"title\"\n                    item-value=\"value\"\n                    label=\"အလွှာ\"\n                    v-model=\"propertyDetail.floor\"\n                  ></v-select>\n\n                  <v-text-field\n                    label=\"တည်ဆောက်သည့်နှစ် / တိုက်သက်တမ်း\"\n                    v-model=\"propertyDetail.year_built\"\n                    autocomplete=\"on\"\n                  ></v-text-field>\n\n                  <v-select\n                    :items=\"regions\"\n                    :item-text=\"'region_mm'\"\n                    :item-value=\"'region_id'\"\n                    v-model=\"propertyDetail.regions_id\"\n                    label=\"Region/State\"\n                    required\n                  >\n                  </v-select>\n\n                  <v-select\n                    :items=\"townships\"\n                    :item-text=\"'townships_mm'\"\n                    :item-value=\"'township_id'\"\n                    v-model=\"propertyDetail.township_id\"\n                    label=\"Township\"\n                    required\n                  >\n                  </v-select>\n\n                  <v-text-field\n                    label=\"Contact Number\"\n                    v-model=\"propertyDetail.contact_number\"\n                    autocomplete=\"on\"\n                    required\n                  ></v-text-field>\n\n                  <v-radio-group v-model=\"bankinstallment\" row>\n                    <span>ဘဏ်အရစ်ကျ &nbsp;&nbsp;</span>\n                    <v-radio label=\"No\" value=\"No\"></v-radio>\n                    <v-radio label=\"Yes\" value=\"Yes\"></v-radio>\n                  </v-radio-group>\n\n                  <v-textarea\n                    name=\"input-7-1\"\n                    label=\"Description\"\n                    v-model=\"propertyDetail.description_mm\"\n                    required\n                  ></v-textarea>\n\n                  <v-textarea\n                    name=\"input-7-1\"\n                    label=\"Description Eng\"\n                    v-model=\"propertyDetail.description_eng\"\n                  ></v-textarea>\n                </div>\n              </v-card-text>\n              <v-card-actions>\n                <v-btn type=\"submit\" block color=\"warning\" dark>\n                  ကြော်ငြာတင်မည်\n                </v-btn>\n              </v-card-actions>\n            </v-card>\n          </form>\n        </v-tab-item>\n      </v-tabs-items>\n    </v-card>\n  </div>\n</template>\n<script>\nimport { HTTP } from \"@/use/http-common\";\n\nexport default {\n  name: \"Adedit\",\n  props: [\"id\"],\n  data() {\n    return {\n      messages: \"\",\n      success: \"\",\n      tab: null,\n\n      propertyDetail: {},\n\n      types: [\n        \"ရောင်းရန်အိမ်ခြံမြေများ\",\n        \"ငှားရန်အိမ်ခြံမြေများ\",\n        \"အိမ်ခြံမြေသစ်များ\",\n      ],\n\n      currency: [\n        {\n          name: \"MMK(Lakhs)\",\n          value: \"MMK(Lakhs)\",\n        },\n        {\n          name: \"USD\",\n          value: \"USD\",\n        },\n      ],\n\n      propertystatus: [\n        {\n          title: \"အလုံးစုံ ပြင်ဆင်ပြီး\",\n          value: \"Fully Furnished\",\n        },\n        {\n          title: \"မပြင်ဆင်ရသေး\",\n          value: \"မပြင်ဆင်ရသေး\",\n        },\n        {\n          title: \"တစ်စိတ်တစ်ပိုင်း ပြင်ဆင်ပြီး\",\n          value: \"တစ်စိတ်တစ်ပိုင်း ပြင်ဆင်ပြီး\",\n        },\n        {\n          title: \"ခန်းမ\",\n          value: \"Hall\",\n        },\n      ],\n\n      areatypes: [\n        {\n          title: \"Sqft\",\n          value: \"Sqft\",\n        },\n        {\n          title: \"Acre\",\n          value: \"Acre\",\n        },\n      ],\n\n      rooms: [\n        {\n          title: \"မပါ\",\n          value: \"0\",\n        },\n        {\n          title: \"1 Room\",\n          value: \"1\",\n        },\n        {\n          title: \"2 Room\",\n          value: \"2\",\n        },\n        {\n          title: \"3 Room\",\n          value: \"3\",\n        },\n        {\n          title: \"4 Room\",\n          value: \"4\",\n        },\n        {\n          title: \"5 Room\",\n          value: \"5\",\n        },\n        {\n          title: \"6 Room\",\n          value: \"6\",\n        },\n        {\n          title: \"7 Room\",\n          value: \"7\",\n        },\n        {\n          title: \"8 Room\",\n          value: \"8\",\n        },\n        {\n          title: \"9 Room\",\n          value: \"9\",\n        },\n        {\n          title: \"10 Room\",\n          value: \"10\",\n        },\n      ],\n\n      floors: [\n        {\n          title: \"မြေညီ\",\n          value: \"Ground Floor\",\n        },\n        {\n          title: \"1 လွှာ\",\n          value: \"1\",\n        },\n        {\n          title: \"2 လွှာ\",\n          value: \"2\",\n        },\n        {\n          title: \"3 လွှာ\",\n          value: \"3\",\n        },\n        {\n          title: \"4 လွှာ\",\n          value: \"4\",\n        },\n        {\n          title: \"5 လွှာ\",\n          value: \"5\",\n        },\n        {\n          title: \"6 လွှာ\",\n          value: \"6\",\n        },\n        {\n          title: \"7 လွှာ\",\n          value: \"7\",\n        },\n        {\n          title: \"8 လွှာ\",\n          value: \"8\",\n        },\n        {\n          title: \"9 လွှာ\",\n          value: \"9\",\n        },\n        {\n          title: \"10 လွှာ\",\n          value: \"10\",\n        },\n        {\n          title: \"11 လွှာ\",\n          value: \"11\",\n        },\n        {\n          title: \"12 လွှာ\",\n          value: \"12\",\n        },\n        {\n          title: \"13 လွှာ\",\n          value: \"13\",\n        },\n        {\n          title: \"14 လွှာ\",\n          value: \"14\",\n        },\n        {\n          title: \"15 လွှာ\",\n          value: \"15\",\n        },\n        {\n          title: \"16 လွှာ\",\n          value: \"16\",\n        },\n        {\n          title: \"17 လွှာ\",\n          value: \"17\",\n        },\n        {\n          title: \"18 လွှာ\",\n          value: \"18\",\n        },\n        {\n          title: \"19 လွှာ\",\n          value: \"19\",\n        },\n        {\n          title: \"20 လွှာ\",\n          value: \"20\",\n        },\n        {\n          title: \"21 လွှာ\",\n          value: \"21\",\n        },\n        {\n          title: \"22 လွှာ\",\n          value: \"22\",\n        },\n        {\n          title: \"23 လွှာ\",\n          value: \"23\",\n        },\n        {\n          title: \"24 လွှာ\",\n          value: \"24\",\n        },\n        {\n          title: \"24 လွှာ\",\n          value: \"24\",\n        },\n        {\n          title: \"25 လွှာ\",\n          value: \"25\",\n        },\n        {\n          title: \"26 လွှာ\",\n          value: \"26\",\n        },\n        {\n          title: \"27 လွှာ\",\n          value: \"27\",\n        },\n        {\n          title: \"28 လွှာ\",\n          value: \"28\",\n        },\n        {\n          title: \"29 လွှာ\",\n          value: \"29\",\n        },\n        {\n          title: \"30 လွှာ\",\n          value: \"30\",\n        },\n      ],\n\n      regions: [],\n      townships: [],\n      propertytype: [],\n\n      title_mm: \"\",\n      title_eng: \"\",\n      property_type_id: \"\",\n      price: \"\",\n      price_type: \"MMK(Lakhs)\",\n      property_status: \"\",\n      area: \"\",\n      area_type: \"Sqft\",\n      room: \"\",\n      bathroom: \"\",\n      bedroom: \"\",\n      livingroom: \"\",\n      region_id: \"\",\n      township_id: \"\",\n      contact_number: \"\",\n      description_mm: \"\",\n      description_eng: \"\",\n      floor: \"\",\n      build_year: \"\",\n      bankinstallment: \"No\",\n\n      form: {\n        title_mm: \"Hello\",\n      },\n    };\n  },\n\n  methods: {\n    swipe(direction) {\n      this.swipeDirection = direction;\n      if (direction) {\n        if (direction === \"Right\") {\n          this.$router.go(-1);\n        }\n      }\n    },\n\n    async fetch_regions() {\n      HTTP.get(`city/index`)\n        .then((response) => {\n          this.regions.push(...response.data.data);\n        })\n        .catch((e) => {});\n    },\n\n    async fetch_township() {\n      HTTP.get(`city/townships`)\n        .then((response) => {\n          this.townships.push(...response.data.data);\n        })\n        .catch((e) => {});\n    },\n\n    async property_type() {\n      HTTP.get(`property_type/index`)\n        .then((response) => {\n          this.propertytype.push(...response.data.data);\n        })\n        .catch((e) => {});\n    },\n\n    async update_property() {\n      if (this.propertyDetail.title_mm == \"\") {\n        this.messages = \"Title ထည့်ပါ\";\n      } else if (this.propertyDetail.property_type_id == \"\") {\n        this.messages = \"Property Type ကို ရွေးပါ\";\n      } else if (this.propertyDetail.regions_id == \"\") {\n        this.messages = \"တိုင်းဒေသကြီးကို ရွေးပါ\";\n      } else if (this.propertyDetail.township_id == \"\") {\n        this.messages = \"မြို့နယ်ကို ရွေးပါ\";\n      } else {\n        HTTP.post(\n          \"property/upda\",\n          {\n            title_mm: this.propertyDetail.title_mm,\n            title_eng: this.propertyDetail.title_eng,\n            property_type_id: this.propertyDetail.property_type_id,\n            price: this.propertyDetail.price,\n            price_type: this.propertyDetail.price_type,\n            property_status: this.propertyDetail.property_status,\n            area: this.propertyDetail.area,\n            area_type: this.propertyDetail.area_type,\n            room: this.propertyDetail.rooms,\n            bathroom: this.propertyDetail.bathroom,\n            bedroom: this.propertyDetail.bead_room,\n            masterbathroom: this.propertyDetail.masterbathroom,\n            livingroom: this.propertyDetail.living_room,\n            region_id: this.propertyDetail.regions_id,\n            township_id: this.propertyDetail.township_id,\n            contact_number: this.propertyDetail.contact_number,\n            description_mm: this.propertyDetail.description_mm,\n            description_eng: this.propertyDetail.description_eng,\n            floor: this.propertyDetail.floor,\n            build_year: this.propertyDetail.year_built,\n            bankinstallment: this.propertyDetail.bankinstallment,\n            sale_id: this.$route.params.id,\n          },\n          {\n            headers: {\n              \"Content-type\": \"application/x-www-form-urlencoded\",\n            },\n          }\n        )\n          .then((response) => {\n            if (response.status === 200) {\n              this.success = \"Update လုပ်ဆောင်ပြီးပါပြီ\";\n            }\n          })\n          .catch((e) => {\n            this.messages = \"အချက်အလက်များကို ထည့်သွင်းပါ။\";\n          });\n      }\n    },\n  },\n\n  created() {\n    HTTP.get(`property/detail/${this.$route.params.id}`)\n      .then((response) => {\n        this.propertyDetail = response.data.data;\n        this.tatnaywon = response.data.tatnaywon;\n        console.log(response.data.data);\n      })\n      .catch((e) => {});\n  },\n\n  mounted() {\n    this.fetch_regions();\n    this.fetch_township();\n    this.property_type();\n  },\n};\n</script>\n"]}]}