v>\n      </v-card-text>\n    </v-card>\n  </div>\n</template>\n\n<script>\nimport { HTTP } from \"@/use/http-common\";\nimport Loader from \"@/components/Loader\";\n\nexport default {\n  name: \"Notification\",\n  data() {\n    return {\n      tab: null,\n      isLoad: true,\n      notifications: [],\n    };\n  },\n\n  methods: {\n    async fetch_notification() {\n      HTTP.get(`notification/index`)\n        .then((response) => {\n          this.notifications.push(...response.data.data);\n          this.isLoad = false;\n          console.log(response.data.data);\n        })\n        .catch((e) => {\n          this.isLoad = true;\n        });\n    },\n  },\n\n  mounted() {\n    this.fetch_notification();\n  },\n\n  components: {\n    Loader,\n  },\n};\n</script>\n<style>\n.v-card--reveal {\n  bottom: 0;\n  opacity: 1 !important;\n  position: absolute;\n  width: 100%;\n}\n</style>"]}]}